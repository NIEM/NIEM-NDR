  <xsd:simpleType name="CaseStatusCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for the kinds of status states for a Case.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Suspended">
        <xsd:annotation>
          <xsd:documentation>The case is no longer active, but can be returned to an active state by a time or event trigger, or user action.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Pending">
        <xsd:annotation>
          <xsd:documentation>The case has been created, but will not become active until triggered by a date-time, event, or user action.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Active-Threat">
        <xsd:annotation>
          <xsd:documentation>The case is active and determined to represent a threat . Requires total commitment of effort to monitor, track, and status.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Active-Hot">
        <xsd:annotation>
          <xsd:documentation>The case is active, but not yet determined to be a threat. Requires constant attention.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Closed-Normal">
        <xsd:annotation>
          <xsd:documentation>The case has been closed.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Unknown">
        <xsd:annotation>
          <xsd:documentation>The case status is unknown.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Inactive">
        <xsd:annotation>
          <xsd:documentation>The case in no longer of interest, but a decision to close the case has not been made.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Closed-Error">
        <xsd:annotation>
          <xsd:documentation>The case has been determined to have been opened in error and has been closed.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Merged">
        <xsd:annotation>
          <xsd:documentation>The case has been closed by merging into another case.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Active-Watch">
        <xsd:annotation>
          <xsd:documentation>The case is active of sufficient interest to maintain on a watch list.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Active-Routine">
        <xsd:annotation>
          <xsd:documentation>The case is active but not being proactively pursued.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>

      </xsd:enumeration>
      <xsd:enumeration value="RADF/HPRDS">
        <xsd:annotation>
          <xsd:documentation>The format for an assessment of data measurements from a HPRDS device.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RMDF/HPRDS">
        <xsd:annotation>
          <xsd:documentation>The format for raw and processed data measurements from a HPRDS device.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RADF/ASP">
        <xsd:annotation>
          <xsd:documentation>The format for an assessment of data measurements from an ASP device.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RMDF/ASP">
        <xsd:annotation>
          <xsd:documentation>The format for raw and processed data measurements from an ASP device.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="NeutronAlarm">
        <xsd:annotation>
          <xsd:documentation>An unresolved neutron alarm has occurred and reachback assistance is requested.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ImageAnalysis">
        <xsd:annotation>
          <xsd:documentation>Initial results of image analysis appear abnormal or suspicious, therefore reachback assistance is requested.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RIID">
        <xsd:annotation>
          <xsd:documentation>Results from RIID scan appear abnormal or suspicious, therefore reachback assistance is requested.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="GammaAlarm">
        <xsd:annotation>
          <xsd:documentation>An unresolved gamma alarm has occurred and reachback assistance is requested.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ASP">
        <xsd:annotation>
          <xsd:documentation>Results from ASP scan appear abnormal or suspicious, therefore reachback assistance is requested.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SuspiciousCargo">
        <xsd:annotation>
          <xsd:documentation>A cargo item has been identified as suspicious, therefore reachback assistance is requested.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Secondary">
        <xsd:annotation>
          <xsd:documentation>An item of interest has been directed to secondary inspection, and reachback assistance is requested.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="ReachbackRequestCodeType">
    <xsd:annotation>
      <xsd:documentation>A data type for the kinds of reachback request.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>

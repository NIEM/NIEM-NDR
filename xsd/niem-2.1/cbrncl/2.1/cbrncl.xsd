  <xsd:simpleType name="DataQualityCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for codes that describe a qualitative assessment of the validity of the data.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Good">
        <xsd:annotation>
          <xsd:documentation>data quality is believed to be acceptable</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Suspect">
        <xsd:annotation>
          <xsd:documentation>data quality is unknown</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Bad">
        <xsd:annotation>
          <xsd:documentation>data quality is known to be unacceptable</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Missing">
        <xsd:annotation>
          <xsd:documentation>data is missing</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="DataQualityCodeType">

    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Luggage">
        <xsd:annotation>
          <xsd:documentation>A package of goods accompanying a person being transported.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Mail">
        <xsd:annotation>
          <xsd:documentation>A mail or courier package item</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Vehicle">
        <xsd:annotation>
          <xsd:documentation>A conveyance that operates on land under its own power.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Package">
        <xsd:annotation>
          <xsd:documentation>A package of goods.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Pipeline">
        <xsd:annotation>
          <xsd:documentation>A continuous line of pipe for conveying liquids over an extended distance.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Ship">
        <xsd:annotation>
          <xsd:documentation>A conveyance for transporting cargo over the oceans and other large bodies of water.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Car">
        <xsd:annotation>
          <xsd:documentation>A conveyance for transporting persons and their personal affects over land via roadways under its own power.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Boat">
        <xsd:annotation>
          <xsd:documentation>A watercraft conveyance typically smaller than a ship and typically employed in roles other than transporting cargo.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Other">
        <xsd:annotation>
          <xsd:documentation>The measured item is of a kind not included in the standard list. It should be described in the element MeasuredItemDescriptionText.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Cargo">
        <xsd:annotation>
          <xsd:documentation>A collection of goods being transported.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Person">
        <xsd:annotation>
          <xsd:documentation>A human being, either living or dead.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Aircraft">
        <xsd:annotation>
          <xsd:documentation>A conveyance for transporting cargo via the airspace above the earths surface.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Railcar">
        <xsd:annotation>
          <xsd:documentation>A conveyance for transporting cargo over land via railways.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Truck">
        <xsd:annotation>
          <xsd:documentation>A conveyance for transporting cargo over land via roadways under its own power.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Container">
        <xsd:annotation>
          <xsd:documentation>Shipping Container</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Trailer">
        <xsd:annotation>
          <xsd:documentation>An unpowered conveyance for transporting cargo over land via roadways.</xsd:documentation>

        <xsd:annotation>
          <xsd:documentation>An encounter focused on determining if biological threat materials are present.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RadNuc">
        <xsd:annotation>
          <xsd:documentation>An encounter focused on determining if radiological or nuclear threat materials are present.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Other">
        <xsd:annotation>
          <xsd:documentation>An other unspecified category of detection event.</xsd:documentation>

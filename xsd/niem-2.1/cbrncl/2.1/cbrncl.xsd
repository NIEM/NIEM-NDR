      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="cbrncl:ConveyanceOrgRelationshipKindCodeSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="ConveyanceRelationshipKindCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for the kinds of relationships that may exist between two conveyances.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="contained in">
        <xsd:annotation>
          <xsd:documentation>A conveyance contained on or in another conveyance, such as a container on a ship or a railcar.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="other">
        <xsd:annotation>
          <xsd:documentation>The relationship between the two conveyances is other than contained in or connected to.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="connected to">
        <xsd:annotation>
          <xsd:documentation>A conveyance connected to another conveyance, such as a trailer connected to the tractor in a truck, or a railcar in a train consist.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

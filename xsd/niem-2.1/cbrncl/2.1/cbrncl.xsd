      <xsd:extension base="cbrncl:AlarmAcknowledgementCodeSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="AlarmDescriptionCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for the kinds of threats that may be annunciated by an alarm.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Runner">
        <xsd:annotation>
          <xsd:documentation>Conveyance or person evaded inspection.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Threat - IED">
        <xsd:annotation>
          <xsd:documentation>A suspected Improvised Explosive Device has been identified.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RIID Request">
        <xsd:annotation>
          <xsd:documentation>Suspect materials - need RIID examination assistance</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Threat-GN">
        <xsd:annotation>
          <xsd:documentation>threat material determined by the gamma ray spectra and the neutron counts</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Threat - Biological">
        <xsd:annotation>
          <xsd:documentation>A suspected biological weapon has been identified.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Threat - IND">
        <xsd:annotation>
          <xsd:documentation>A suspected Improvised Nuclear Device or nuclear weapon has been identified.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Critical Help">
        <xsd:annotation>
          <xsd:documentation>Situation critical - officer needs immediate assistance</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Threat - RDD">
        <xsd:annotation>
          <xsd:documentation>A suspected Improvised Radiation Dispersal Device has been identified.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Threat-Neutron">
        <xsd:annotation>
          <xsd:documentation>threat material determined by the neutron counts</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Error">
        <xsd:annotation>
          <xsd:documentation>device not producing proper alarm data</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Suspect">
        <xsd:annotation>
          <xsd:documentation>undetermined radioactive material that may be associated with a threat device</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Threat - Chemical">
        <xsd:annotation>
          <xsd:documentation>A suspected chemical weapon has been identified.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Other">
        <xsd:annotation>
          <xsd:documentation>Other</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Threat-Gamma">
        <xsd:annotation>
          <xsd:documentation>threat material determined by the gamma ray spectra</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Alarm Cancelled">
        <xsd:annotation>
          <xsd:documentation>the alarm is cancelled</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Innocent">
        <xsd:annotation>
          <xsd:documentation>routine radioactive material not associated with a threat device</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Personnel Hazard">
        <xsd:annotation>
          <xsd:documentation>radiation dose rates pose a threat to personal safety</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

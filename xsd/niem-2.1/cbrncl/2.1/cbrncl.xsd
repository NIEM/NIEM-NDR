  <xsd:simpleType name="ConveyanceOrgRelationshipKindCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for the kinds of relationships that may exist between a conveyance and an organization.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="owner">
        <xsd:annotation>
          <xsd:documentation>The organization owns the conveyance, but does not operate it.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="owner/operator">
        <xsd:annotation>
          <xsd:documentation>The organization owns and operates the conveyance.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="lessee">
        <xsd:annotation>
          <xsd:documentation>The organization leases (or rents) the conveyance from a lessor.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="operator">
        <xsd:annotation>
          <xsd:documentation>The organization operates the conveyance, but does not own it.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="other">
        <xsd:annotation>
          <xsd:documentation>The organizations relationship to the conveyance is other than own/operate/lease/rent.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="lessor">
        <xsd:annotation>
          <xsd:documentation>The organization leases (or rents) the conveyance to lessee.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="ConveyanceOrgRelationshipKindCodeType">
    <xsd:annotation>

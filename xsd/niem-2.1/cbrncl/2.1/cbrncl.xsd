    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="CoordinatesMethodCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for the methods by which the coordinates of a location are determined.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Site">
        <xsd:annotation>
          <xsd:documentation>The coordinates have been determined by survey of a fixed site.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Map">
        <xsd:annotation>
          <xsd:documentation>The coordinates have been determined by manual methods using a map that includes a georef coordinate grid.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Estimate">
        <xsd:annotation>
          <xsd:documentation>The coordinates are an estimate.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Other">
        <xsd:annotation>
          <xsd:documentation>The coordinates have been determined by an unspecified method.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Unknown">
        <xsd:annotation>
          <xsd:documentation>The method of determining the coordinates is unknown.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="EquipmentOther">
        <xsd:annotation>
          <xsd:documentation>The coordinates have been determined by position locating equipment other than GPS and Loran.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Loran">
        <xsd:annotation>
          <xsd:documentation>The coordinates have been determined by a Loran device.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="GPS">
        <xsd:annotation>
          <xsd:documentation>The coordinates have been determined by a GPS device.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

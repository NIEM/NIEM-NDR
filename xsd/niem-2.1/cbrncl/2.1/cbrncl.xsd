  <xsd:simpleType name="AlarmAcknowledgementCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for the kinds of alarm acknowledgements.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Neutron">
        <xsd:annotation>
          <xsd:documentation>Acknowledges a neutron radiation alarm.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Neutron-Gamma">
        <xsd:annotation>
          <xsd:documentation>Acknowledges a neutron and/or gamma radiation alarm.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="False Alarm">
        <xsd:annotation>
          <xsd:documentation>Acknowledges an indication of a false alarm.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Other">
        <xsd:annotation>
          <xsd:documentation>Other</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="None">
        <xsd:annotation>
          <xsd:documentation>Indicates that an alarm did not occur; invalid acknowledgement.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Gamma">
        <xsd:annotation>
          <xsd:documentation>Acknowledges a gamma radiation alarm.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="AlarmAcknowledgementCodeType">

    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Chem">
        <xsd:annotation>
          <xsd:documentation>An encounter focused on determining if chemical threat materials are present.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>

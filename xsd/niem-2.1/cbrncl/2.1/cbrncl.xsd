  <xsd:simpleType name="EncounterDeviceHealthStatusCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for codes that describe the health status of an encounter device.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Planned">
        <xsd:annotation>
          <xsd:documentation>The device is not a real device.  It represents a planned device at some point in the future.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Stored">
        <xsd:annotation>
          <xsd:documentation>The device is in storage. It is not deployed nor in maintenance.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Depot">
        <xsd:annotation>
          <xsd:documentation>The device is in a maintenance facility and not operationally deployed.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Down">
        <xsd:annotation>
          <xsd:documentation>The device is out of operations in a fault condition; it is not capable of supporting operational activities.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Disposed">
        <xsd:annotation>
          <xsd:documentation>The device no longer exists in the inventory. It has been destroyed, scrapped or otherwise disposed of.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Transport">
        <xsd:annotation>
          <xsd:documentation>The device is in transit between deployment sites and is not mission capable.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Operational">
        <xsd:annotation>
          <xsd:documentation>The device is fully mission capable to include communications networks.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Suspended">
        <xsd:annotation>
          <xsd:documentation>The device is deployed but not mission capable for reasons other than a fault condition; it is not capable of supporting operational activities.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Unknown">
        <xsd:annotation>
          <xsd:documentation>The state of the device is unknown.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Degraded">
        <xsd:annotation>
          <xsd:documentation>The device's capabilities are degraded, but critical functions and communications are working.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Off-line">
        <xsd:annotation>
          <xsd:documentation>The device is mission capable, but network communications are not functioning.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PM">
        <xsd:annotation>
          <xsd:documentation>The device is mission capable, but not currently in operation use because it is undergoing inspection, calibration, certification, and/or preventive maintenance. If necessary, it could be rapidly returned to operation use.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Retired">
        <xsd:annotation>
          <xsd:documentation>The device has been retired from active use but has not been disposed.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>

      <xsd:documentation>An association between a vessel and a person on board other than a crewmember (e.g., passenger, maintenance person, stowaway).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselOfficialCoastGuardNumberText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An Official United States Coast Guard Number (USCG Official Number) of a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselOperationalConditionOfEquipment" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for the Operational Condition of Equipment (OCE) of a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:VesselOperationalConditionOfEquipment" name="VesselOperationalConditionOfEquipmentDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the Operational Condition of Equipment (OCE) of a vessel.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="VesselOperationalConditionOfEquipment"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselOperator" type="nc:EntityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An entity that is responsible for the day-to-day operations and travels of a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselOriginLocation" type="m:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The origin of a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselOverallLengthMeasure" type="nc:LengthMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A measure of the overall length of a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselOwner" type="nc:EntityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An entity that owns a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselPersonAssociation" type="m:VesselPersonOnBoardAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An association between a vessel and a person on board the vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselPersonRole" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a role that a non-crewmember plays aboard a vessel (e.g., passenger, maintenance person, stowaway).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:VesselPersonRole" name="VesselPersonRoleText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A role that a non-crew person plays aboard a vessel (e.g., passenger, maintenance person, stowaway).</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="VesselPersonRole"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselPortAssociation" type="m:VesselPortAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An association between a vessel and a port.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselPosition" type="m:VesselPositionType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A geospatial position, course, heading, speed, and status of a vessel at a given time.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselPositionDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date and time that a position of a vessel was recorded or measured.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselPurposeOfVisit" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a reason why a vessel visits a port.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:VesselPurposeOfVisit" name="VesselPurposeOfVisitText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the reason why a vessel visits a port.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="VesselPurposeOfVisit"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselRampQuantity" type="nc:QuantityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The number of ramps a vessel has.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselRateOfTurnMeasure" type="m:AngularRateMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A measurement of the rate of turn of a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselReceivingFacilityName" type="nc:ProperNameTextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A name of a facility that will receive a vessel while at a port.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselRoleAtPort" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a role a work vessel fulfills at a port.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:VesselRoleAtPort" name="VesselRoleAtPortText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the role a work vessel fulfills at a port.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="VesselRoleAtPort"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselSCONUMText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A Ship Control Number (SCONUM) of a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselSafetyManagementCertificate" type="m:CertificateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A Safety Management Certificate (SMC) for a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselSecurityOfficerContactInformation" type="nc:ContactInformationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The contact information for the appointed Company Security Officer (CSO) of a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselSecurityPlanImplementedIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if a vessel has implemented a Vessel Security Plan (VSP); false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselShaftQuantity" type="nc:QuantityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The number of shafts a vessel has.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselSpeedMeasure" type="nc:SpeedMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A measure of the speed of a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselStackQuantity" type="nc:QuantityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The number of stacks on a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselStateIdentification" type="nc:IdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An identification of a vessel given by a government.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselSuperstructureColor" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for the primary color of the superstructure of a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:VesselSuperstructureColor" name="VesselSuperstructureColorCode" type="fbi:VCOCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The primary color of the superstructure of a vessel.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="VesselSuperstructureColor"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:VesselSuperstructureColor" name="VesselSuperstructureColorText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The primary color of the superstructure of a vessel.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="VesselSuperstructureColor"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselSuperstructureLocation" type="m:LocationOnVesselType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The location of a superstructure on a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselTRDM" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A US Transportation Command (USTRANSCOM) Reference Data Model (TRDM) unique identifier for a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselTrackReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A track of a vessel, captured as a series of geospatial positions over time.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="VesselTrackType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselTrack" type="m:VesselTrackType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A track of a vessel, captured as a series of geospatial positions over time.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselTrackBoundingArea" type="geo:EnvelopeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The geographic area encompassing the set of vessel positions in a vessel track.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselTrackBoundingDateRange" type="nc:DateRangeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The dates and times (inclusive) encompassing the set of vessel positions in a vessel track.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselVoyage" type="m:VoyageType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A journey of a vessel from one place to another.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VolumeUnit" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a unit of measure of a volume value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:VolumeUnit" name="VolumeUnitText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A unit of measure of a volume value.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="VolumeUnit"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VoyageReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A journey from one place to another.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="VoyageType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Voyage" type="m:VoyageType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A journey from one place to another.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VoyageBeginDateTime" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date and time a voyage begins.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VoyageCategory" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a kind of a voyage.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:VoyageCategory" name="VoyageCategoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A kind of a voyage.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="VoyageCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VoyageDestinationLocation" type="m:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The destination of a voyage.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VoyageEndDateTime" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date and time a voyage ends.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VoyageIdentification" type="nc:IdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An identification for a voyage name, number, or code.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VoyageManifestAssociation" type="m:VoyageManifestAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An association between a voyage and a manifest.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VoyageOriginLocation" type="m:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The origin of a voyage.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VoyagePortOfCallAssociation" type="m:VesselPortAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An association between a voyage and a port visited between the origin and destination of the voyage.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VoyageSummaryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A summary or description of a voyage.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VoyageVesselUseText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the use or purpose of a vessel on a voyage.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:WatchListEntity" name="WatchListAircraftReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>An aircraft on a watch list.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="AircraftType"/>
        <i:Base i:name="WatchListEntity"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="WatchListAssociation" type="m:WatchListAssociationType" nillable="true">

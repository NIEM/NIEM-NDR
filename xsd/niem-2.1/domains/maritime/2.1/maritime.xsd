      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VesselCrewOnBoardAssociationType">
    <xsd:annotation>
      <xsd:documentation>A data type for an association between a vessel and a crew member on board the vessel.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="VesselPersonOnBoardAssociationType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="m:VesselPersonOnBoardAssociationType">
        <xsd:sequence>
          <xsd:element ref="m:CrewRole" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VesselEquipmentOnBoardAssociationType">
    <xsd:annotation>
      <xsd:documentation>A data type for an association between a vessel and equipment on board the vessel.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="AssociationType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:AssociationType">
        <xsd:sequence>
          <xsd:element ref="m:VesselReference" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:EquipmentReference" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:LocationOnVessel" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VesselLocationAssociationType">
    <xsd:annotation>
      <xsd:documentation>A data type for an association between a vessel and a location.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="ConveyanceLocationAssociationType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="m:ConveyanceLocationAssociationType">
        <xsd:sequence>
          <xsd:element ref="m:VesselNavigationStatusAbstract" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselRateOfTurnMeasure" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VesselManifestAssociationType">
    <xsd:annotation>
      <xsd:documentation>A data type for an association between a vessel and a manifest.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="AssociationType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:AssociationType">
        <xsd:sequence>
          <xsd:element ref="m:VesselReference" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:ManifestReference" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VesselMovementDetailsType">
    <xsd:annotation>
      <xsd:documentation>A data type for the details of the geospatial movement of a vessel.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="m:VesselActualActivityAbstract" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselDestinationLocation" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselIntendedActivityAbstract" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselOriginLocation" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselTrack" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VesselNonCrewOnBoardAssociationType">
    <xsd:annotation>
      <xsd:documentation>A data type for an association between a vessel and a person on board other than a crew member (e.g., passenger, maintenance person, stowaway).</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="VesselPersonOnBoardAssociationType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="m:VesselPersonOnBoardAssociationType">
        <xsd:sequence>
          <xsd:element ref="m:PersonVisitingAddressLocation" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:RecordLocatorIdentification" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselPersonRole" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VesselPersonOnBoardAssociationType">
    <xsd:annotation>
      <xsd:documentation>A data type for an association between a vessel and a person on board the vessel. This is an abstract type.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="AssociationType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:AssociationType">
        <xsd:sequence>
          <xsd:element ref="m:VesselReference" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="nc:PersonReference" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:PersonDebarkationDate" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:PersonDebarkationLocation" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:PersonEmbarkationDate" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:PersonEmbarkationLocation" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VesselPortAssociationType">
    <xsd:annotation>
      <xsd:documentation>A data type for an association between a vessel and a port.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="AssociationType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:AssociationType">
        <xsd:sequence>
          <xsd:element ref="m:VesselReference" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:PortReference" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VoyageReference" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselActualArrivalDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselActualDepartureDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselAnchorageText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselExpectedArrivalDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselExpectedDepartureDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselLocationInPort" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselPurposeOfVisit" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselReceivingFacilityName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselRoleAtPort" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VesselPositionType">
    <xsd:annotation>
      <xsd:documentation>A data type for a geospatial position, course, heading, speed, and status of a vessel at a given time.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="m:LocationPointAbstract" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselSpeedMeasure" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselCourseMeasure" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselHeadingMeasure" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselNavigationStatusAbstract" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselPositionDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselRateOfTurnMeasure" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VesselTrackType">
    <xsd:annotation>
      <xsd:documentation>A data type for describing the track of a vessel, captured as a series of geospatial positions over time.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="m:VesselTrackBoundingArea" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselTrackBoundingDateRange" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VesselPosition" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VolumeMeasureType">
    <xsd:annotation>
      <xsd:documentation>A data type for a measurement of volume.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="MeasureType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:MeasureType">
        <xsd:sequence>
          <xsd:element ref="m:VolumeUnit" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VoyageManifestAssociationType">
    <xsd:annotation>
      <xsd:documentation>A data type for an association between a voyage and a manifest.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="AssociationType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:AssociationType">
        <xsd:sequence>
          <xsd:element ref="m:VoyageReference" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:ManifestReference" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VoyageType">
    <xsd:annotation>
      <xsd:documentation>A data type for a journey from one place to another.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="m:VoyageBeginDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VoyageCategory" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VoyageDestinationLocation" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VoyageEndDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VoyageIdentification" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VoyageOriginLocation" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VoyageSummaryText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:VoyageVesselUseText" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="WatchListAssociationType">
    <xsd:annotation>
      <xsd:documentation>An association between an organization and an entity (vessel, aircraft, person, etc.) that the organization declares to be of interest and has placed on a watch list.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="AssociationType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:AssociationType">
        <xsd:sequence>
          <xsd:element ref="nc:OrganizationReference" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:WatchListEntity" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:WatchListEntryAction" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:WatchListEntryActionDeadlineDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:WatchListEntryConcern" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:WatchListEntryContactInformation" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:WatchListEntryEffectiveStartDate" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:WatchListEntryEffectiveStopDate" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:WatchListEntryPriority" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:WatchListEntryReasonOfInterest" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:WatchListEntryThreatLevel" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:WatchListEntryTitle" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:WatchListEntryUpdateDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="m:WatchListEntryURI" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="ActivityConveyanceInvolvementText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the involvement of a conveyance with an activity or event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ActivityInvolvedConveyanceAssociation" type="m:ActivityInvolvedConveyanceAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An association between an activity or event and a conveyance involved in that activity or event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AngleUnit" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a unit of measure of an angle value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:AngleUnit" name="AngleUnitText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A unit of measure of an angle value.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AngleUnit"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AngularRateUnit" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a unit of measure of an angular rate of change.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:AngularRateUnit" name="AngularRateUnitText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A unit of measure of an angular rate of change.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AngularRateUnit"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AnomalousConveyanceInvolvementText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of how a conveyance was involved in an anomaly.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AnomalyReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>An out-of-the-ordinary occurrence.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="AnomalyType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Anomaly" type="m:AnomalyType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An out-of-the-ordinary occurrence.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AnomalyCategory" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a type or kind of anomaly.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:AnomalyCategory" name="AnomalyCategoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A type or kind of anomaly.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AnomalyCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AnomalyContactInformation" type="nc:ContactInformationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A Point of Contact (POC) for further information regarding an anomaly.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AnomalyConveyanceAssociation" type="m:AnomalyConveyanceAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An association between an anomaly and a conveyance that was involved in the anomaly or that performed the anomalous behavior.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AnomalyDateTime" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date and time an anomaly occurred.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AnomalyDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of an anomaly.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AnomalyLocation" type="m:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The location where an anomaly occurred.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="COTPZone" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a Captain of the Port (COTP) zone or region.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:COTPZone" name="COTPZoneCode" type="m:COTPRegionNameType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A Captain of the Port (COTP) zone code.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="COTPZone"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="COTPZoneContactInformation" type="nc:ContactInformationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A Point Of Contact (POC) for a Captain of the Port (COTP) zone.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="COTPZoneGoverningEntity" type="nc:EntityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An organization that is responsible for a Captain of the Port (COTP) zone.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:COTPZone" name="COTPZoneText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A name of a Captain of the Port (COTP) zone.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="COTPZone"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaptainOfThePortZone" type="m:CaptainOfThePortZoneType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A geographical area or zone where a Coast Guard Captain of the Port (COTP) enforces port safety and security.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A good or goods transported by a conveyance.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="CargoType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Cargo" type="m:CargoType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A good or goods transported by a conveyance.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoArrivalDate" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date and time cargo arrived at a port.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoCategory" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a kind or category of cargo.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:CargoCategory" name="CargoCategoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A kind or category of cargo.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="CargoCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoChargeableWeightMeasure" type="nc:WeightMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A measure of the gross weight or mass of cargo on which a charge is to be based.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoCustomsValueAmount" type="nc:AmountType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A monetary value of cargo as it relates to customs and importation.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoDepartureDate" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date and time cargo left a port.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoDestinationLocation" type="m:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The intended destination of cargo.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoDiameterMeasure" type="nc:LengthMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A measure of the diameter of cargo.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoDrainedWeightMeasure" type="nc:WeightMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A measure of the drained weight of cargo.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoGrossVolumeMeasure" type="m:VolumeMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A measure of the total gross volume of cargo.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoGrossWeightMeasure" type="nc:WeightMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A measure of the total weight or mass of cargo.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoHazardousMaterialIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if cargo contains hazardous material; false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoHazmatDeclaration" type="m:HazmatDeclarationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A declaration for hazardous materials in cargo.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoIdentification" type="nc:IdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An identifier for cargo.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoItemReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A good transported by a conveyance.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="CargoItemType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoItem" type="m:CargoItemType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A good transported by a conveyance.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoLoadingDate" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date and time cargo was loaded onto a conveyance.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoLoadingLocation" type="m:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The location of a conveyance when cargo was loaded on board.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoLocationInPort" type="nc:LocationType" nillable="true">

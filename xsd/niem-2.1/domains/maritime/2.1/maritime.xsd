    <xsd:annotation>
      <xsd:documentation>The date and time at which an entity (vessel, aircraft, person, etc.) begins participation on a watchlist.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="WatchListEntryEffectiveStopDate" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date and time at which an entity (vessel, aircraft, person, etc.) ends participation on a watchlist.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="WatchListEntryPriority" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a priority or level of importance for an entity (vessel, aircraft, person, etc.) on a watch list.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:WatchListEntryPriority" name="WatchListEntryPriorityText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A priority or level of importance for an entity (vessel, aircraft, person, etc.) on a watch list.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="WatchListEntryPriority"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="WatchListEntryReasonOfInterest" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A reason why a watch list entry is of interest.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="WatchListEntryThreatLevel" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a level of threat posed by an entity (vessel, aircraft, person, etc.) on a watch list.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:WatchListEntryThreatLevel" name="WatchListEntryThreatLevelText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A level of threat posed by an entity (vessel, aircraft, person, etc.) on a watch list.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="WatchListEntryThreatLevel"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="WatchListEntryTitle" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The title of a watchlist entry.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="WatchListEntryURI" type="niem-xsd:anyURI" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A URI for a watchlist entry where additional information may be found.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="WatchListEntryUpdateDateTime" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A date and time when a watchlist entry was updated.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:WatchListEntity" name="WatchListOrganizationReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>An organization on a watch list.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="OrganizationType"/>
        <i:Base i:name="WatchListEntity"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:WatchListEntity" name="WatchListPersonReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A person on a watch list.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="PersonType"/>
        <i:Base i:name="WatchListEntity"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:WatchListEntity" name="WatchListVehicleReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A vehicle on a watch list.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="VehicleType"/>
        <i:Base i:name="WatchListEntity"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:WatchListEntity" name="WatchListVesselReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A vessel on a watch list.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="VesselType"/>
        <i:Base i:name="WatchListEntity"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
</xsd:schema>

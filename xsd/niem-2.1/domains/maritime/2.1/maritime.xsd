      <xsd:documentation>A navigational status of a vessel.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="VesselNavigationStatusAbstract"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselNavigationStatusAbstract" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a navigational status of a vessel.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:VesselNavigationStatusAbstract" name="VesselNavigationStatusText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A navigational status of a vessel.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="VesselNavigationStatusAbstract"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VesselNetTonnageVolumeMeasure" type="m:VolumeMeasureType" nillable="true">

      <xsd:documentation>An association between an organization and an entity (vessel, aircraft, person, etc.) that the organization declares to be of interest and has placed on a watch list.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:WatchListEntity" name="WatchListConveyanceReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A conveyance on a watch list.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ConveyanceType"/>
        <i:Base i:name="WatchListEntity"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="WatchListEntity" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a physical entity (e.g., vessel, aircraft, person) on a watch list.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="WatchListEntryAction" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a requested action for an observer to take upon observing or encountering the entity (vessel, aircraft, person, etc.) on a watch list.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="WatchListEntryActionDeadlineDateTime" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date and time which the action must be performed by or else it becomes irrelevant.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:WatchListEntryAction" name="WatchListEntryActionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A requested action for an observer to take upon observing or encountering the entity (vessel, aircraft, person, etc.) on a watch list.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="WatchListEntryAction"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="WatchListEntryConcern" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a level of concern for an entity (vessel, aircraft, person, etc.) on a watch list.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="m:WatchListEntryConcern" name="WatchListEntryConcernText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A level of concern for an entity (vessel, aircraft, person, etc.) on a watch list.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="WatchListEntryConcern"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="WatchListEntryContactInformation" type="nc:ContactInformationType" nillable="true">

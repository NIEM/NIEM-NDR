  <xsd:complexType name="TribalCodeType">
    <xsd:annotation>
      <xsd:documentation>A list of the names of the tribes.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="fs:TribalCodeSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="VisitationPlanType">
    <xsd:annotation>
      <xsd:documentation>A structure containing the details about a current visitation agreement</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="fs:ChangesMadeAffectingVisitationRightsText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="fs:VisitationDuration" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="j:VisitationSupervisingOfficial" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="fs:VisitationTime" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:attribute name="childSupportEnforcementCaseIdentifierStatusText" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation>An attribute that shows the status of a IV-D case identifier assigned by a local agency to a child-support-enforcement case ("IV-D" refers to Title IV-D of the Social Security Act).</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:element name="AFCARSPermanencyGoalCode" type="fs:AFCARSPermanencyGoalCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Indicates the AFCARS  (Adoption and Foster Care Analysis and Reporting System) Permanency Goal that most closely describes a service plan goal.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ActionRequested" abstract="true">
    <xsd:annotation>
      <xsd:documentation>The action being requested by the agency.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="fs:ActionRequested" name="ActionRequestedCode" type="fs:ActionRequestedCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Code indicating any of the actions being requested of the court e.g.
  what types of services the child needs
  (e.g. mental health, medical, emergency,
  basic, therapeutic, habilitative).</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="ActionRequested"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="fs:ActionRequested" name="ActionRequestedText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Text describing the action being requested.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="ActionRequested"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ActivityInvolvedPersonAssociation" type="fs:ActivityInvolvedPersonAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Yes indicates that this person is required to sign the Service Plan</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:Address" name="Address" type="fs:AddressType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:AddressType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="Address"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AddressAugmentation" type="fs:AddressAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about an address.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="AddressType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AgencyCase" type="nc:CaseType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Contains child welfare agency  case details.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AgencyCaseNumber" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The case number created by the organization.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AgencyRecommendationsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the recommendations of the agency for any changes to the service plan, continued placement, and requests for new court dates.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="j:Charge" name="Allegation" type="j:ChargeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Details about a formal allegation that a specific person has abused or neglected a child.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/domains/jxdm/4.1" i:name="Charge"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AllegationAugmentation" type="fs:ChargeAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about an allegation.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/domains/jxdm/4.1" i:name="ChargeType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AllegationCategory" abstract="true">
    <xsd:annotation>
      <xsd:documentation>The category of an allegation.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="fs:AllegationCategory" name="AllegationCategoryCode" type="fs:AllegationCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Code indicating NCANDS (National Child Abuse and Neglect Data System) allegation categories.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AllegationCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="fs:AllegationCategory" name="AllegationCategoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the allegation.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AllegationCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AllegationReportedDate" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date the allegation was reported.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AssignedToPersonAssociation" type="nc:PersonAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The person a task is assigned to.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AssociatedPersonReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A person who is associated with the child.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="AssociatedPersonType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AssociatedPerson" type="fs:AssociatedPersonType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A person who is associated with the child.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AssociatedPersonChildAssociation" type="fs:AssociatedPersonChildAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information about the association between an associated person and a child.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="j:JudicialOfficial" name="Attorney" type="fs:AttorneyType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type j:JudicialOfficialType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/domains/jxdm/4.1" i:name="JudicialOfficial"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AttorneyAugmentation" type="fs:AttorneyAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about an attorney.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/domains/jxdm/4.1" i:name="JudicialOfficialType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Caregiver" type="fs:CaregiverType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A person who takes care of another.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseInitiationReason" abstract="true">
    <xsd:annotation>
      <xsd:documentation>The reason the case was initiated.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="fs:CaseInitiationReason" name="CaseInitiationReasonCode" type="fs:CaseInitiationReasonCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A codified list of the reason  the case was initiated (new filing, reopening,  transfer in, severance).</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="CaseInitiationReason"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="fs:CaseInitiationReason" name="CaseInitiationReasonText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Reason regarding a specific child. Examples might be physical abuse, sexual abuse, physical neglect, medical
  neglect, etc.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="CaseInitiationReason"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseInvolvementDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of how a person is involved in the case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseInvolvementText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of a person's involvement in the underlying child welfare case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Caseworker" type="fs:CaseworkerType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The caseworker for the child.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseworkerCategory" abstract="true">
    <xsd:annotation>
      <xsd:documentation>The type of caseworker assigned to the case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="fs:CaseworkerCategory" name="CaseworkerCategoryCode" type="fs:CaseworkerCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Code indicating the type of caseworker assigned e.g. investigation, ongoing, adoption, permanency, caseworker
  supervisor.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="CaseworkerCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="fs:CaseworkerCategory" name="CaseworkerCategoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Text describing the type of caseworker e.g. ongoing, initial, etc.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="CaseworkerCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChangesMadeAffectingVisitationRightsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The changes made by the court that affect current visitation rights.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="j:Charge" name="Charge" type="fs:ChargeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type j:ChargeType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/domains/jxdm/4.1" i:name="Charge"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChargeAugmentation" type="fs:ChargeAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about a charge.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/domains/jxdm/4.1" i:name="ChargeType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A data type for information about a person who has not yet reached the age of legal majority (i.e., adulthood) and who is the subject of a child-welfare or child-support proceeding.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="ChildType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Child" type="fs:ChildType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A data type for information about a person who has not yet reached the age of legal majority (i.e., adulthood) and who is the subject of a child-welfare or child-support proceeding.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildRemovalDate" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date the child was removed from caregiver's custody.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportArrearsPaymentAmount" type="nc:AmountType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The amount the child-support obligor is ordered to pay at a specified frequency (e.g., weekly, monthly) toward the total arrearages amount.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportAssignedArrearsAmount" type="nc:AmountType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The portion of total arrears identified in the child-support order as being attributable to recovery from the obligor of assigned arrears.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:Case" name="ChildSupportEnforcementCase" type="fs:ChildSupportEnforcementCaseType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:CaseType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="Case"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportEnforcementCaseAugmentation" type="fs:ChildSupportEnforcementCaseAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about a child-support-enforcement case.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="CaseType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:CaseDispositionDecision" name="ChildSupportEnforcementCaseDispositionDecision" type="fs:ChildSupportEnforcementCaseDispositionDecisionType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:CaseDispositionDecisionType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="CaseDispositionDecision"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportEnforcementCaseDispositionDecisionAugmentation" type="fs:ChildSupportEnforcementCaseDispositionDecisionAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about the disposition of a child-support-enforcement case.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="CaseDispositionDecisionType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportEnforcementCaseDispositionDecisionCode" type="fs:ChildSupportEnforcementCaseDispositionDecisionCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of disposition decisions that a tribunal can reach at the conclusion of a hearing, trial, or other proceeding in a child-support-enforcement case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportEnforcementCaseDispositionDecisionFindingsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the tribunal's factual findings in support of its disposition decision in a child-support-enforcement case (e.g., the tribunal does or does not have jurisdiction over the case, respondent is or is not the child's father, respondent's actual or imputed income, etc.).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportEnforcementCaseOriginatorAssociation" type="fs:ChildSupportEnforcementCaseOriginatorAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A relationship between a child-support-enforcement agency and the child-support-enforcement case it initiated.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportEnforcementCaseOriginatorIdentification" type="nc:IdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The unique alphanumeric code assigned to the child-support-enforcement agency that initiated the child-support-enforcement case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportEnforcementCaseTransferCode" type="fs:ChildSupportEnforcementCaseTransferCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of reasons why a child-support-enforcement case was transferred.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportEnforcementIV-DCaseCategoryIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the child support enforcement case implicates Title IV-D of the Social Security Act (e.g., the custodial parent has received funds from the Temporary Assistance for Needy Families (TANF) program); otherwise, false.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportEnforcementIV-DCaseIdentification" type="nc:IdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A unique value assigned by a local agency to identify and track each IV-D case in the state, tribe, or territory ("IV-D" refers to Title IV-D of the Social Security Act).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:Organization" name="ChildSupportEnforcementOrganization" type="fs:ChildSupportEnforcementOrganizationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:OrganizationType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="Organization"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportEnforcementOrganizationAugmentation" type="fs:ChildSupportEnforcementOrganizationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about a child-support-enforcement agency.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="OrganizationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportEnforcementOrganizationIdentification" type="nc:IdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A unique alphanumeric value assigned locally to each child-support-enforcement agency (e.g., State Disbursement Unit Identifier).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportFeesAmount" type="nc:AmountType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The amount the child-support obligor is ordered to pay for the fees incurred in the case (e.g., attorney fees, filing fees, fees for paternity testing, etc.).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportInsuranceBeneficiaryPersonAssociation" type="fs:ChildSupportInsuranceBeneficiaryPersonAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An association between insurance coverage ordered in a child-support-enforcement case and the person (usually a child) who is the beneficiary of that insurance.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:Insurance" name="ChildSupportInsuranceCoverageOrder" type="fs:ChildSupportInsuranceCoverageOrderType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:InsuranceType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="Insurance"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportInsuranceCoverageOrderAugmentation" type="fs:ChildSupportInsuranceCoverageOrderAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Information about the insurance coverage ordered in a child-support-enforcement case.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="InsuranceType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportInsuranceCoverageOrderCategory" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Details about the kind(s) of insurance coverage ordered in a child-support-enforcement case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="fs:ChildSupportInsuranceCoverageOrderCategory" name="ChildSupportInsuranceCoverageOrderCode" type="fs:ChildSupportInsuranceCoverageCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of the kind(s) of insurance coverage ordered in a child-support-enforcement case.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="ChildSupportInsuranceCoverageOrderCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="fs:ChildSupportInsuranceCoverageOrderCategory" name="ChildSupportInsuranceCoverageOrderText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the kind(s) of insurance coverage ordered in a child-support-enforcement case.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="ChildSupportInsuranceCoverageOrderCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportNonTANFArrearsAmount" type="nc:AmountType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The portion of total arrears identified in the child-support order as being attributable to child-support payments that were due from the obligor in the past but were not paid.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:Obligation" name="ChildSupportObligation" type="fs:ChildSupportObligationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:ObligationType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="Obligation"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportObligationAugmentation" type="fs:ChildSupportObligationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about the monetary and nonmonetary provisions of a child support order.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ObligationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportObligationOrderActionCode" type="fs:ChildSupportObligationOrderActionCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of legal actions a child-support-enforcement tribunal can take regarding a child-support obligation.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportTANFArrearsAmount" type="nc:AmountType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The portion of total arrears identified in the child-support order as being attributable to recovery from the obligor of the Temporary Assistance for Needy Families (TANF) funds previously provided for the child.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportTotalArrearsAmount" type="nc:AmountType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The total arrearages amount identified in the child-support order.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportUnreimbursedExpenseDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the unreimbursed expenses for which the child-support obligor is financially responsible (e.g., uncovered medical costs, education costs, etc.).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildSupportUnreimbursedExpensePercentage" type="nc:PercentageType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The percentage the child-support obligor is ordered to pay for unreimbursed expenses (e.g., uncovered medical costs, education costs, etc.).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildVictim" type="fs:ChildVictimType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A person under the age of 18.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildVictimCaseAssociation" type="fs:ChildVictimCaseAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the relationship between a child and their victim (e.g., step parent, father, mother, etc.)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildVictimChargeAssociation" type="fs:ChildVictimChargeAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the relationship between a child and their victim (e.g., step parent, father, mother, etc.)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildWelfareAgencyCaseIdentifier" type="nc:IdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A unique alphanumeric value assigned by a state or local child welfare agency to track a dependency or neglect case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="j:JudicialOfficial" name="ChildWelfareAttorney" type="fs:ChildWelfareAttorneyType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type j:JudicialOfficialType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/domains/jxdm/4.1" i:name="JudicialOfficial"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildWelfareAttorneyAugmentation" type="fs:ChildWelfareAttorneyAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about an attorney who represents a party in a dependency or neglect case.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/domains/jxdm/4.1" i:name="JudicialOfficialType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildWelfareCounselForWhomText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the client represented by an attorney involved in a dependency or neglect case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:Organization" name="ChildWelfareOrganization" type="fs:ChildWelfareOrganizationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:OrganizationType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="Organization"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ChildWelfareOrganizationAugmentation" type="fs:ChildWelfareOrganizationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about a child welfare agency.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="OrganizationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CompetencyStatusText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes whether a person is competent to understand the court process</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CounselForWhomText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Text stating who this attorney is representing.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CountyOfCustodyText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the county in which a child resides</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CourtCase" type="nc:CaseType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An element that contains the details of a court case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CourtEventAttendee" type="fs:CourtEventAttendeeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A person attending a court event</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CourtEventInputIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the person provided input during the event, false if otherwise</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="j:CourtOrder" name="CourtOrder" type="fs:CourtOrderType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type j:CourtOrderType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/domains/jxdm/4.1" i:name="CourtOrder"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CourtOrderAugmentation" type="fs:CourtOrderAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about the court order.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/domains/jxdm/4.1" i:name="CourtOrderType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CourtOrderRenderedDate" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date the judge verbally issued the order.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CriminalHistoryPendingChargesText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Includes all criminal history, pending cases, current location (Soldiers Sailors Relief Act)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CriteriaForSuccessText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes what needs to be done to successfully complete a task.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CurrentStatusSummaryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes progress toward accomplishment of this task</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CustodyStatusText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Who has custody of the child, e.g. TMC Department or JTMC Dept and Parent.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DentalHistoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Can include diagnosis, treatment plans, medications</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:Disposition" name="Disposition" type="fs:DispositionType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:DispositionType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="Disposition"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DispositionAugmentation" type="fs:DispositionAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about a disposition.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="DispositionType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DivorcePendingIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if a divorce proceeding is pending; otherwise, false.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Education" type="fs:EducationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the educational issues related to the court report and service plan of any participant in the case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EducationOrganizationReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>An education organization that currently has a juvenile enrolled as a student.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="OrganizationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EducationalGoalsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes a kind of educational level the child and or court wishes to
  achieve.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EducationalNeedsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of specific educational needs that the child may have.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EnglishInterpreterRequiredIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if any of the case participants requires an interpreter during case proceedings; otherwise, false.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:ImmediateFamilyAssociation" name="FamilyAssociation" type="fs:FamilyAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:ImmediateFamilyAssociationType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ImmediateFamilyAssociation"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FamilyAssociationAugmentation" type="fs:FamilyAssociationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about a family association.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ImmediateFamilyAssociationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FamilyIdentifier" type="nc:IdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A unique alphanumeric value assigned to a family for the purposes of discovering and connecting related records.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FatherUnknownText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A statement as to whether or not the father is unknown.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FederalFindingsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Text about federal findings.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Findings" type="fs:FindingsType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Details about the findings of the court.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="GangOrganizationReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A criminal gang organization that is alleged to have a juvenile as a member.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="OrganizationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="GoalObjective" type="fs:GoalObjectiveType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes a subset of a service plan goal.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="HistoricalNarrative" type="fs:HistoricalNarrativeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the history of a participant of a case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="HistoryDetailsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Details regarding the history of the case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="HouseholdMemberIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the associated person is a household member, false if otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:ImmediateFamilyAssociation" name="ImmediateFamilyAssociation" type="fs:ImmediateFamilyAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:ImmediateFamilyAssociationType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ImmediateFamilyAssociation"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ImmediateFamilyAssociationAugmentation" type="fs:ImmediateFamilyAssociationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about an immediate family association (parents and their children).</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ImmediateFamilyAssociationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="IncomeAttachmentIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the child-support order authorizes attachment of the obligor's income; otherwise, false.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="IndigencyStatus" type="nc:StatusType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Yes indicates that the court has found the person to be in financial need and can not afford their own legal
  representation.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="InstantFindingsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Text describing the instant findings.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenileCurrentEducationDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the juvenile's current educational program (e.g., grade level, curriculum, etc.).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenileCurrentEducationEnrollmentEndDate" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date a juvenile's current educational program is scheduled to conclude.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenileCurrentEducationEnrollmentIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the juvenile is currently enrolled in an educational program; otherwise, false.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenileCurrentEducationTruancyIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the juvenile is currently considered truant; otherwise, false.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenileEducationOrganizationAssociation" type="fs:JuvenileEducationOrganizationAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An association between a juvenile and the education organization where the juvenile is enrolled as a student.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenileGangAssociation" type="fs:JuvenileGangAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An association between a juvenile and a criminal gang organization.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenileGangAssociationDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the alleged association between a juvenile and a criminal gang organization.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenileGangMemberReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A juvenile who is alleged to be associated with a criminal gang organization.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="PersonType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenileInformalAdjustmentIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the juvenile (with parental consent) admits the facts of the alleged delinquent act, and the tribunal approves the conditions of the juvenile's supervision for a period of time without being adjudicated; otherwise, false.  (Informal adjustment is primarily used for first-time offenders and is ordered without a hearing.)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenilePlacedReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>The juvenile who is directed to reside somewhere other than a parent's residence.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="PersonType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenilePlacement" type="fs:JuvenilePlacementType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information about where a juvenile is directed to reside during the pendency of the delinquency proceeding.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenilePlacementFacilityAssociation" type="fs:JuvenilePlacementFacilityAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An association between a juvenile and a facility where the juvenile is directed to reside (e.g., orphanage, teen shelter, detention center, etc.).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenilePlacementGoalText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the intended outcome of the placement (e.g., reunification, termination of parental rights, etc.).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenilePlacementOtherLocationAssociation" type="fs:JuvenilePlacementOtherLocationAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A data type for an association between a juvenile and a location -- other than a foster home, kin's residence, orphanage, or other facility -- where the juvenile is directed to reside.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenilePlacementPersonAssociation" type="fs:JuvenilePlacementPersonAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An association between a juvenile and a person with whom the juvenile is directed to reside (e.g., foster parent, grandparent, etc.).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenilePlacementSupervisorAssociation" type="fs:JuvenilePlacementSupervisorAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An association between a juvenile and an entity responsible for supervising the juvenile's placement (e.g., social services department, probation department, etc.).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenileSpecialEducationEligibleIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the juvenile is eligible for enrollment in a special-education program (based on diagnosed developmental disabilities, learning disorders, etc.); otherwise, false.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenileStatusOffenseIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the alleged act is an offense when committed by a juvenile but not when committed by an adult; otherwise, false.  (i.e., acts such as truancy, violating curfew, running away from home, and the like are not offenses unless the offender is under the age of majority.)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="JuvenileStudentReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A juvenile who is currently enrolled in an educational program.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="PersonType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="LocalAllegationDescriptionText" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A state or local definition of an allegation.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MaritalSeparationDate" type="niem-xsd:date" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date a husband and wife stop residing in the same home.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:MarriageAssociation" name="MarriageAssociation" type="fs:MarriageAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:PersonUnionAssociationType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="MarriageAssociation"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MarriageAssociationAugmentation" type="fs:MarriageAssociationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about a marriage association.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="PersonUnionAssociationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MedicalHistoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Can include diagnosis, treatment plans, medications</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MedicalStatus" type="fs:MedicalStatusType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the medical history text, dental history text, psychological history text, and Substance Abuse History Text.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MilitaryInductionDate" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A date a person was inducted (or will be inducted) into active service in a military.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:MilitarySummary" name="MilitarySummary" type="fs:MilitarySummaryType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:MilitarySummaryType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="MilitarySummary"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MilitarySummaryAugmentation" type="fs:MilitarySummaryAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about a person's military service.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="MilitarySummaryType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NextCourtEvent" type="j:CourtEventType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes an upcoming court occurrence.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NonCriminalLegalIssuesText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes other cases and situations that may impact a court report. Tribal membership status and other ICWA issues. Includes prior juvenile (non-criminal) history, child support enforcement issues, mental health adjudications. Also identifies interstate placement issues (where there is an ICPC issue or UCCJEA issue). Also indicates the citizenship and legal residency status (if applicable)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="j:Offense" name="Offense" type="fs:OffenseType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type j:OffenseType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/domains/jxdm/4.1" i:name="Offense"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OffenseAugmentation" type="fs:OffenseAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about an offense.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/domains/jxdm/4.1" i:name="OffenseType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:Organization" name="Organization" type="fs:OrganizationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:OrganizationType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="Organization"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OrganizationAugmentation" type="fs:OrganizationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about an organization.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="OrganizationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OrganizationCaseNumber" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The case number created  by the organization.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Participant" type="fs:ParticipantType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A person who is involved in a case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PaternityDeterminationDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Any additional description of the details about the determination of paternity, such as findings regarding the location and date of conception, the results of DNA tests, and the like.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PaternityDeterminationIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if paternity has been established between a man and his alleged child; otherwise, false.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PaternityDeterminationMethodCode" type="fs:PaternityDeterminationMethodCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of methods by which paternity is legally established.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PermanencyAchievementDate" type="nc:ActivityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the date that the Permanency Goal is to be achieved.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:Person" name="Person" type="fs:PersonType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A human being.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="Person"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PersonAugmentation" type="fs:PersonAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about a person.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="PersonType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PersonCaseAssociation" type="fs:PersonCaseAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the relationship between a person and a given case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Petition" type="fs:PetitionType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A formally drawn request, often bearing the names of a number of those making the request, that is addressed to a person or group of persons in authority or power, soliciting some favor, right, mercy, or other benefit.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PetitionCategory" abstract="true">
    <xsd:annotation>
      <xsd:documentation>The type of petition being submitted.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="fs:PetitionCategory" name="PetitionCategoryCode" type="fs:PetitionCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A code indicating the type  of petition being submitted.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="PetitionCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="fs:PetitionCategory" name="PetitionCategoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The type of petition being submitted.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="PetitionCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PetitionID" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An ID which identifies a  petition.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PetitionReasonText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Text describing the reason for the petition.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PetitionerReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>The person filing the petition.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="PetitionerType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Petitioner" type="fs:PetitionerType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The person filing the petition.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PetitionerChildAssociation" type="fs:PetitionerChildAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information about the association between a petitioner and a child.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Placement" type="fs:PlacementType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the placement history of the child</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PlacementCategoryRestrictivenessText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A text field to indicate how the placement type is the least restrictive placement necessary.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PlacementCategoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the type of placement the child is entering.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PlacementChangeReasonText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Text describing what the reason was for the placement change, e.g.  reunification, level of care lowered.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PlacementFacilityReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>The facility where a juvenile is directed to reside.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="FacilityType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PlacementOtherLocationReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>The location -- other than a foster home, kin's residence, orphanage, or other facility -- where the juvenile is directed to reside.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="LocationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PlacementPersonReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>The person with whom a juvenile is directed to reside.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="PersonType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PlacementSupervisorReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>The entity who is responsible for supervising the juvenile's placement (e.g., social services department, probation department, etc.).</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="OrganizationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PlacementText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Where the child was placed,  e.g. Parents, foster case, respite care.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PresentingProblemText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Presenting history of the circumstances surrounding removal of the child. Includes some analysis of how those circumstances put the child in danger or actually harmed the child.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PrimaryGoalIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Yes indicates that this is the primary goal of this service plan.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:Program" name="Program" type="fs:ProgramType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:ProgramType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="Program"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ProgramAugmentation" type="fs:ProgramAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about a program.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ProgramType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ProgramEligibleEndDate" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A date a person lost eligibility for a program (e.g., exceeded income limits, violated a program rule, or otherwise failed to satisfy a requirement).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ProgramEligibleStartDate" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A date a person became eligible to enroll in a program (e.g., receive financial assistance, treatment, or some other social service).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ProgramMonetaryValue" type="nc:ItemValueType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An evaluation of the monetary worth of the program provided.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PsychologicalHistoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Can include diagnosis, treatment plans, medications</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ReasonForInitialRemovalText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the primary reason that the child was removed from the home.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ReasonableEffortsDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the steps and activities that the agency/state done to achieve permanency goal.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RelatedCase" type="fs:RelatedCaseType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A case which is related  in some way to another.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RelativeHomeStudiesText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>What the agency is doing for kinship care.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RelativePlacementResources" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Name of relatives that are available and able to care for child.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RemovalIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the child was removed, false if otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RequiredParty" type="fs:RequiredPartyType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A person who is required  to be present at the next court event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResponsibilityForServiceCostText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The agency responsible for paying the cost of a service.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResponsibleEntityDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the services  that agency will provide such as arrangement  of transportation, enrollment, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Service" type="fs:ServiceType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the service  that will be made available by the agency.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ServiceCostValue" type="nc:NumericType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The cost or estimated cost of the service.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ServiceDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the service that will be made available by the agency.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ServicePlanGoal" type="fs:ServicePlanGoalType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Elements that describe a service plan goal.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SignatureRequiredIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>If value is set to yes then a signature is required from this person.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="StateDisbursementIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the child-support order directs the obligor to make payments to a state agency for disbursement to the custodial parent; otherwise, false.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="StatutoryFindingsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Text describing the statutory findings.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="StudentIdentification" type="nc:IdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A unique number or set of alphanumeric characters assigned to a student by an education organization.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SubjectChildAssociation" type="fs:SubjectChildAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information about an association between a subject and a child.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SubmissionDateTime" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A date/time of an activity  that occurs at a singular point in time or a start date/time of an activity that occurs over a period of time.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SubmittedDateTime" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date and time the document  was submitted.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SubstanceAbuseHistoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Can include diagnosis, treatment plans, medications</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SupportingGroundsDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the legal basis for the child-support remedies sought in the petition (e.g., respondent is non-custodial parent and is failing to provide support, a change in the parties' circumstances justifies a modification, etc.).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Task" type="fs:TaskType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the activity to be done to achieve a service plan objective.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TimeframeDescription" type="nc:ActivityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the date range a task needs to be completed by.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TimeframeDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the date range a task needs to be completed by.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TribalAffiliationText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The tribe in which the child may be affiliated with.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TribalCode" type="fs:TribalCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of the names of the tribes.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VisitationDuration" type="niem-xsd:duration" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the period of time allowed for a visitation with the format of PnYnMnDTnHnMnS, where nY is the number of years, nM is the number of months, nD is the number of days, nH is the number of hours, nM is the number of minutes, and nS is the number of seconds.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VisitationPlan" type="fs:VisitationPlanType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Contains the details about a current visitation agreement.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VisitationTime" type="niem-xsd:time" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Indicates the time of day that the visitation is to take place.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
</xsd:schema>

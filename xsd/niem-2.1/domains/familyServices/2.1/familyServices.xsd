  <xsd:simpleType name="ChildSupportEnforcementCaseDispositionDecisionCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A list of disposition decisions that a tribunal can reach at the conclusion of a hearing, trial, or other proceeding in a child-support-enforcement case.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="FinalOrder">
        <xsd:annotation>
          <xsd:documentation>The tribunal entered a final order regarding all of the child-support remedies requested in the petition.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="GeneticTestOrder">
        <xsd:annotation>
          <xsd:documentation>The tribunal ordered the childs alleged kin to submit to genetic testing for the purpose of determining paternity.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TemporaryOrder">
        <xsd:annotation>
          <xsd:documentation>The tribunal entered a temporary order regarding some or all of the child-support remedies requested in the petition.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Other">
        <xsd:annotation>
          <xsd:documentation>The tribunal entered an unenumerated order.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DismissedWithPrejudice">
        <xsd:annotation>
          <xsd:documentation>The child-support-enforcement petition was dismissed with prejudice (e.g., respondent was found not to be the childs parent, the petition exceeded the number of child-support-modification requests permitted by the jurisdiction during a time period, etc.).</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="EnforcementOrder">
        <xsd:annotation>
          <xsd:documentation>The tribunal entered an order to enforce a related child-support order (e.g., garnishment, contempt, license suspension, etc.)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DismissedWithoutPrejudice">
        <xsd:annotation>
          <xsd:documentation>The child-support-enforcement petition was dismissed without prejudice (e.g., respondent was not served with process within a time limit, petition was not filed with proper tribunal, etc.).</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="ChildSupportEnforcementCaseDispositionDecisionCodeType">
    <xsd:annotation>
      <xsd:documentation>A list of disposition decisions that a tribunal can reach at the conclusion of a hearing, trial, or other proceeding in a child-support-enforcement case.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="fs:ChildSupportEnforcementCaseDispositionDecisionCodeSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="ChildSupportEnforcementCaseDispositionDecisionType">
    <xsd:annotation>
      <xsd:documentation>A data type for an applied augmentation for type nc:CaseDispositionDecisionType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="CaseDispositionDecisionType"/>
        <i2:AugmentedTypeIndicator>true</i2:AugmentedTypeIndicator>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:CaseDispositionDecisionType">
        <xsd:sequence>

  <xsd:simpleType name="PaternityDeterminationMethodCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A list of methods by which paternity is legally established.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="TribalCeremony">
        <xsd:annotation>
          <xsd:documentation>Paternity was legally established through a tribes ceremonial proceeding.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="StipTest">
        <xsd:annotation>
          <xsd:documentation>Paternity was legally established because, following publication of the results of a paternity test, the alleged father stipulated that he is the childs parent.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Admin">
        <xsd:annotation>
          <xsd:documentation>Paternity was legally established through an administrative process.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Other">
        <xsd:annotation>
          <xsd:documentation>Paternity has been legally established through an unenumerated method.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Default">
        <xsd:annotation>
          <xsd:documentation>Paternity was legally established because the alleged father failed to appear in a state court proceeding and the court entered a default order.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Order">
        <xsd:annotation>
          <xsd:documentation>Paternity was legally established at the conclusion of a state court proceeding in which the court found the alleged father to be the childs parent and entered an order.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Acknowledgement">
        <xsd:annotation>
          <xsd:documentation>Paternity was legally established through the alleged fathers voluntary acknowledgement that he is the childs father.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TribalOrder">
        <xsd:annotation>
          <xsd:documentation>Paternity was legally established through a tribal courts process.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Stipulation">
        <xsd:annotation>
          <xsd:documentation>Paternity was legally established because the alleged father stipulated that he is the childs parent.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="PaternityDeterminationMethodCodeType">
    <xsd:annotation>
      <xsd:documentation>A list of methods by which paternity is legally established.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>

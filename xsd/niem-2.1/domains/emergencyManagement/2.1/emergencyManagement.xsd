  </xsd:element>
  <xsd:element name="NotificationDetailsIncidentID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The group listing naming the referent incident(s) of the alert message. Used to collate multiple messages referring to different aspects of the same incident
</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NotificationDetailsKeywordText" type="nc:TextType" nillable="true">

  </xsd:element>
  <xsd:element name="NotificationDetailsDocument" type="nc:DocumentType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The human-readable text describing the notification. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NotificationDetailsIncidentDescriptionText" type="nc:TextType" nillable="true">

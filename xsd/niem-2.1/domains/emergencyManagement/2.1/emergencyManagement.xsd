  <xsd:element name="AlarmAudibleIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if an audible alarm is sounding; false if the alarm is inaudible.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmCallBackTelephoneNumber" type="nc:TelephoneNumberType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The phone number of the monitoring station.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmCategory" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Text describing the type of the event which resulted in an alarm.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmConfirmation" type="em:AlarmConfirmationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The container for information related to the confirmation of an alarm.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmConfirmationCallPrivacyBypassPasscodeText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The code allows the Public Safety Answering Point or monitoring station to bypass any call screening and directly reach the alarm subscriber.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmConfirmationCallToPremisesIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if an agency is to call the premise before arrival; false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmConfirmationCallToPremisesResultsDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>This field represents the results of the actions taken by the alarm company to attempt to reach someone at the premise before notifying the Public Safety Answering Point.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmConfirmationMethodDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>General mechanism used to confirm the validity of the alarm. e.g., observed video, live audio, guard verified, call to premises etc.  If the alarm is confirmed through a call to the premise, then the details of this call are indicated in the Call to Premise text field</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmDetailsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Additional details about the alarm event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmDispatchActivity" type="nc:ActivityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The appropriate response or activity needed when a dispatch agency is activated in response to an alarm.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmDispatchAgency" abstract="true">
    <xsd:annotation>
      <xsd:documentation>The name of the agency (PSAP:Public Safety Answering Point) being requested to respond to an alarm  (typically a police or fire department or central dispatch point) or responder.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlarmDispatchAgency" name="AlarmDispatchAgencyID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An alphanumeric that uniquely identifies the agency (PSAP) being requested to respond to an alarm  (typically a police or fire department or central dispatch point) or responder.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlarmDispatchAgency"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlarmDispatchAgency" name="AlarmDispatchAgencyNameText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The name of the agency (PSAP) being requested to respond to an alarm  (typically a police or fire department or central dispatch point) or responder.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlarmDispatchAgency"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmEvent" type="em:AlarmEventType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An alarm event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmEventCallBackTelephoneNumber" type="nc:TelephoneNumberType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A telephone number of the alarm event requestor.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmEventCallPrivacyBypassCode" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A code necessary to access the sites phone system.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmEventCallToPremisesIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if an agency is to call the premise before arrival; false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmEventCategory" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A kind of alarm event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlarmEventCategory" name="AlarmEventCategoryCode" type="apco:AlarmEventCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A kind of alarm event.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlarmEventCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlarmEventCategory" name="AlarmEventCategoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Text describing the type of the event which resulted in an alarm.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlarmEventCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmEventDetailsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Additional details about the alarm event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmEventDispatchAgency" type="nc:OrganizationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An agency to which an alarm event is dispatched.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmEventLocationAugmentation" type="em:AlarmEventLocationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about a location.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="LocationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmEventLocationCategory" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A kind of location at which an alarm event occurs.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlarmEventLocationCategory" name="AlarmEventLocationCategoryCode" type="apco:AlarmEventLocationCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A kind of location at which an alarm event occurs.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlarmEventLocationCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlarmEventLocationCategory" name="AlarmEventLocationCategoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A kind of location at which an alarm event occurs.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlarmEventLocationCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmEventPermit" type="em:PermitType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A permit for the alarm event site.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmEventRequestorReceiveDate" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A date the requestor received the alarm event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmEventResponseAction" abstract="true">
    <xsd:annotation>
      <xsd:documentation>An action requested of an alarm event responder.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlarmEventResponseAction" name="AlarmEventResponseActionCode" type="apco:AlarmEventResponseActionCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An action requested of an alarm event responder.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlarmEventResponseAction"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlarmEventResponseAction" name="AlarmEventResponseActionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An action requested of an alarm event responder.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlarmEventResponseAction"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:Location" name="AlarmLocation" type="em:AlarmLocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:LocationType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="Location"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmLocationAugmentation" type="em:AlarmLocationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information related to the location of the alarm.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="LocationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmLocationCategoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Type of property that has been alarmed (e.g. commercial, residential).  Determines how alert is broadcast (e.g. holdup alarm if commercial, duress alarm if residential).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmLocationDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Necessary information about the site. Alert information that may be provided to the responder. e.g., electrified fence, dogs on property, loft apartment, multi-storied building, multiple warehouses on site, hazardous material etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmLocationDirectionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Directions to the site.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmMonitoringServiceProvider" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Organization contacted when an alarm </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlarmMonitoringServiceProvider" name="AlarmMonitoringServiceProviderIdentification" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A unique number assigned to Central Station Monitoring Companies.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlarmMonitoringServiceProvider"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlarmMonitoringServiceProvider" name="AlarmMonitoringServiceProviderNameText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The name of the dispatch requesting agency (typically an alarm central station).</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlarmMonitoringServiceProvider"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmMonitoringServiceProviderReceiveDateTime" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date and time the monitoring station received the event based on the time zone of the site.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmPermit" type="em:AlarmPermitType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A permit for the alarm event site.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmRequestedResponseActionDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An action requested of an alarm event responder.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>Notification of an incident or threat that may require a coordinated response or action. A type of NOTIFICATION.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="AlertType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Alert" type="em:AlertType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Notification of an incident or threat that may require a coordinated response or action. A type of NOTIFICATION.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:Location" name="AlertAffectedLocation" type="em:AlertAffectedLocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:LocationType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="Location"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertAffectedLocationAugmentation" type="em:AlertAffectedLocationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>The text describing the affected area of the alert message.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="LocationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertCategoryCode" type="em:AlertCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The code  value denoting the type of alert message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertDistributionScopeAudienceDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The text describing the intended audience of the alert message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertEffectiveDateTime" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The effective time of the information of the alert message.
</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertEventDetails" type="em:AlertEventDetailsType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The container for all component parts of the info sub-element of the alert message. Multiple occurrences are permitted within a single alert.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertEventDetailsAudienceDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The text describing the intended audience of the alert message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertEventDetailsCategory" abstract="true">
    <xsd:annotation>
      <xsd:documentation>The category of the subject event of the alert message. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlertEventDetailsCategory" name="AlertEventDetailsCategoryCode" type="em:AlertEventDetailsCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The code denoting the category of the subject event of the alert message. </xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlertEventDetailsCategory"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertEventDetailsCertainty" abstract="true">
    <xsd:annotation>
      <xsd:documentation>The certainty of the subject event of the alert message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlertEventDetailsCertainty" name="AlertEventDetailsCertaintyCode" type="em:AlertEventDetailsCertaintyCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The code denoting the certainty of the subject event of the alert message.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlertEventDetailsCertainty"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertEventDetailsDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The human-readable text describing the incident/event/situation associated with the Alert message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertEventDetailsHeadlineText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The text headline of the alert message. A brief human-readable headline. Note that some displays (for example, short messaging service devices) may only present this headline; it SHOULD be made as direct and actionable as possible while remaining short. 160 characters MAY be a useful target limit for headline length.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertEventDetailsInstructionsDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The text describing the recommended action to be taken by recipients of the alert message. An extended human readable instruction to targeted recipients. (If different instructions are intended for different recipients, they should be represented by use of multiple info blocks. See CAP specification for details.)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertEventDetailsOnsetDateTime" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The expected time of the beginning of the subject event of the alert message.
</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertEventDetailsResponseRecommendation" abstract="true">
    <xsd:annotation>
      <xsd:documentation>The type of action recommended for the target audience.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlertEventDetailsResponseRecommendation" name="AlertEventDetailsResponseRecommendationCode" type="em:AlertEventDetailsResponseRecommendationCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The code denoting the type of action recommended for the target audience.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlertEventDetailsResponseRecommendation"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertEventDetailsSeverityCode" type="em:AlertEventDetailsSeverityCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The code denoting the severity of the subject event of the alert message. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertEventDetailsSystemRecordID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A system specific code identifying the event type of the alert message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertEventDetailsUrgency" abstract="true">
    <xsd:annotation>
      <xsd:documentation>The urgency of the subject event of the alert message.
</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlertEventDetailsUrgency" name="AlertEventDetailsUrgencyCode" type="em:AlertEventDetailsUrgencyCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The code denoting the urgency of the subject event of the alert message.
</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlertEventDetailsUrgency"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertExpirationDateTime" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The expiry time of the information of the alert message. If this item is not provided, each recipient is free to set its own policy as to when the message is no longer in effect.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertHandling" abstract="true">
    <xsd:annotation>
      <xsd:documentation>The code denoting the special handling of the alert message</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlertHandling" name="AlertHandlingIdentification" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The code denoting the special handling of the alert message</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlertHandling"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:AlertHandling" name="AlertHandlingText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The code denoting the special handling of the alert message.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="AlertHandling"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertHeadlineText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The text headline of the alert message. A brief human-readable headline. Note that some displays (for example, short messaging service devices) may only present this headline; it SHOULD be made as direct and actionable as possible while remaining short. 160 characters MAY be a useful target limit for headline length.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertIncidentAssociation" type="em:AlertIncidentAssociationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The group listing identifying related alert messages and incidents. Used to collate multiple messages referring to different aspects of the same incident.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertInstructionsDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The text describing the recommended action to be taken by recipients of the alert message. An extended human readable instruction to targeted recipients. If different instructions are intended for different recipients, they should be represented by use of multiple info blocks.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertReasonText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The text describing the reason the alert message was distributed.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlertSupplementaryDigitalResource" type="em:AlertSupplementaryDigitalResourceType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A digital representation of an object.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AnesthesiaServiceCoverageStatusCode" type="have-codes:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of anesthesia services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ArrivalDate" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date that a resource arrived at the route location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ArrivalLocation" type="nc:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A textual description of the location where the specified resource has arrived at dateTime of the resource message when transported to or from an Incident.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AvailabilityText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>See: http://www.fema.gov/nims/mutual_aid.shtm - Text or code indicating availability of an item</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AvailableQuantity" type="nc:QuantityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The number of vacant/available beds to which patients can be immediately transported. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="BaselineQuantity" type="nc:QuantityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The maximum (baseline) number of beds in this category</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="BedCapacity" type="em:BedCapacityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The number of available beds.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="BedCategoryCode" type="have-codes:BedCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Enumerated list of available Bed Types.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="BinaryChecksumDigest" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The code representing the digital digest ('hash') computed from the resource file.  Calculated using the Secure Hash Algorithm (SHA-1) per [FIPS 180-2].</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="BurnServiceCoverageStatusCode" type="have-codes:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of burn center services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Capacity" type="em:CapacityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Container element to define the capacity information of each specified bed kind or sub category bed kind.  </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CapacityStatusCode" type="have-codes:CapacityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Indicator of status of bed kind or sub-category bed kind. </xsd:documentation>
    </xsd:annotation>

        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Private">
        <xsd:annotation>
          <xsd:documentation>For dissemination only to specified addresses (see address, below)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Restricted">
        <xsd:annotation>
          <xsd:documentation>For dissemination only to users with a known operational requirement (see restriction, below)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Public">
        <xsd:annotation>
          <xsd:documentation>For general dissemination to unrestricted audiences</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>

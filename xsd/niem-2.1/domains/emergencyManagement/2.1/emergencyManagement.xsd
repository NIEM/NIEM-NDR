  <xsd:element substitutionGroup="em:NotificationSource" name="NotificationSourceID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The identification string of the particular source of this alert; e.g., an operator or a specific device.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="NotificationSource"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:NotificationSource" name="NotificationSourceNameText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The name, in text, of particular source of this alert; e.g., an operator or a specific device.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="NotificationSource"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NotificationStatus" type="em:NotificationStatusType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The code denoting the appropriate handling of the notification message; identifies if the notification is an actual event, test, exercise or internal system function.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NotificationStatusCode" type="em:NotificationStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The actionability of the message. 
</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:Location" name="NotificationTargetLocation" type="em:NotificationTargetLocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:LocationType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="Location"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NotificationTargetLocationAugmentation" type="em:NotificationTargetLocationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information related to the target location of the notification.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="LocationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OBGYN" type="em:OBGYNType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of OBGYN services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OBGYNGeneralServiceCoverageStatusCode" type="have-codes:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>It is an indicator specific to OBGYN coverage.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OBGYNServiceCoverageStatusCode" type="have-codes:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The Sub-type element of the OBGYN services. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OrganizationGeoLocation" type="em:OrganizationGeoLocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The container element for specifying the geo-coded address. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OrganizationIDProviderName" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The name of the provider that has provided the identification scheme. This could also be the name a particular identification list. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OrganizationLocationAugmentation" type="em:OrganizationLocationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Additional information about the location of an organization.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="LocationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OriginatingMessageIdentification" type="nc:IdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Each EDXL resource message contains an identifier that uniquely identifies each resource message. This element identifies the ResourceMessageID of the original message to which this message refers. This Message ID is an EDXL-RM Message ID, not an EDXL-DE </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OrthopedicServiceCoverageStatusCode" type="have-codes:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of orthopedic services. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OrthopedicsServiceCoverageStatusCode" type="have-codes:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of Orthopedic surgical services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Owner" type="nc:OrganizationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An organization with a specific range of authority for specified resource(s)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OwnerInformation" type="em:OwnerInformationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An indicator of ownership that can be a: role, person, organizational unit, Department, or Entity name to define ownership.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PediatricServiceCoverageStatusCode" type="have-codes:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Sub-type element of the psychiatric services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PediatricsGeneralServiceCoverageStatusCode" type="have-codes:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of Pediatrics general surgical services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Permit" type="em:PermitType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A permit.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PermitCategoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A kind of permit. In conjunction with the permit number, this indicates the type of alarm (e.g. fire, burglary).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PermitIdentification" type="nc:IdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The alarm permit number for the site.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Psychiatric" type="em:PsychiatricType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of psychiatric services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PsychiatricGeneralServiceCoverageStatusCode" type="have-codes:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>It is an indicator specific to Psychiatric coverage.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ReimplantationServiceCoverageStatusCode" type="have-codes:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of reimplantation surgical services.  </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ReportingInstructionsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The name of the party that the requested item is to report to when they arrive at the incident.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RequestAcceptDeclineCode" type="edxl:ResponseCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Used to accept or decline a Request, Response, Unsolicited Offer, or a Request Return.  Decline indicates the request, response or offer is not accepted.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RequestResponse" type="em:RequestResponseType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Further defines a response specific to: Resource, Resource Message, ResourceMessageType and this response can be an acceptance , declination or a return request pertaining to a specific message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Resource" type="em:ResourceType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Contains one or more defining attributes and any value from a discrete managed list that references the resource.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceActualArrivalDate" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Actual date of arrival of the resource a Report To Location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceActualDepartureDate" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Actual date when the resource departs from its current location for transit to a Report To Location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceAnticipatedIncidentAssignmentText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Anticipated function, task, job, or role to be provided by the requested resource.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceAnticipatedReturnDate" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>When the resource is expected to be returned or DEMOBed.  Completed for a Request Resource.  This element with the Requested Arrival Date / Time provides the estimated duration of resource deployment.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceAvailableDate" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>When the resource is available for use.  Completed in response to a resource message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceCommittedDate" type="nc:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>When specified resource is committed to a request or order.  Completed in response to a resource message.  Specified resource is no longer available to be applied to other resource requests</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceCurrentLocation" type="nc:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The location where the specified resource is physically located at the time of the request.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceDepartureLocation" type="nc:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The location where the specified resource is departing from when transported to or from an Incident.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Free Text alternative to ResourceKeyword (or may be used in addition).  Any description of resource or resource characteristics, situation requiring resource assistance, statement of mission resource must satisfy, or RFI text.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceDispositionKeywordValue" type="em:ValueType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Any value from a discrete managed list, used to specify the general state of a resource if known., in the form:
&lt;ResourceStatuskeyword&gt;
  &lt;valueListUrn&gt;valueListUrn&lt;/valueListURN&gt;
  &lt;value&gt;value&lt;/value&gt;
&lt;/ResourceStatuskeyword&gt;
where the content of valueListUrn is the Uniform Resource Name of a published list of values and definitions, and the content of value is a string denoting the value itself .  Multiple instances MAY occur.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceEstimatedArrivalDate" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>When the resource is expected to arrive at its Report To Location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceEstimatedDepartureDate" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>When the resource is expected to depart from its current location for transit to a Report-to Location</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceFundCode" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifies the funds that will pay for the resource - Identified in support of NIMS Resource Management Guide NIC-GDL0004</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceIdentification" type="nc:IdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>This identifier (if available) must follow the resource / resource description during the entire trail of messages about that resource.  May be selected via the ResourceKeyword if available.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceInformation" type="em:ResourceInformationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>It contains further definition regarding the resource(s) identified in the message and as such may contain a discrete list , numerical value(s), ID or textual descriptions of resources.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceJurisdictionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A geopolitical area in which an organization, person, or object has a specific range of authority for specified resources</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceKeywordValue" type="em:ValueType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Any value from a discrete managed list, used to specify a resource, in the form:
&lt;resourcekeyword&gt;
  &lt;valueListUrn&gt;valueListUrn&lt;/valueListURN&gt;
  &lt;value&gt;value&lt;/value&gt;
&lt;/resourcekeyword&gt;
where the content of valueListUrn is the Uniform Resource Name of a published list of values and definitions, and the content of value is a string denoting the value itself.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceKind" type="em:ResourceKindType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A definition or description of a Resource.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceLocationDetails" type="em:ResourceLocationDetailsType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information that can include textual descriptions, physical location of resource, departure location of Resource, location need for Resource and/or for tracking.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceMessageStatusKeywordValue" type="em:ValueType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description or Code used to identify the message status of a resource message.  
Any value from a discrete managed list, used to specify a Request Status.  The content of valueListUrn is the Uniform Resource Name of a published list of values and definitions, and the content of value (StatusText) is a string denoting the value itself .  Multiple instances MAY occur within a single ResourceDataFrame block.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceModeOfTransportationText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Method or mode used to transport the resource to or from the incident.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceName" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A name or title of the resource.  May be selected via the ResourceKeyword if available.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceOrderIdentification" type="nc:IdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Number or ID assigned by the ordering system or personnel meeting the request for resources that has been made.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourcePriceQuoteAmount" type="nc:AmountType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Quoted cost to acquire a desired resource.  Complete in response to a Request Resource Quote</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceQuantity" type="niem-xsd:nonNegativeInteger" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Number of resources</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceReportToLocation" type="nc:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Where the requested resource is to report-to, or checks in at the destination.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceRequestedArrivalDate" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>When the resource is needed.  Completed for Resource requests, returns, etc.  </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceRequestedLocation" type="nc:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Where the requested resource is needed.  Completed for Resource requests, returns, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceRestrictionsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of a condition governing the availability of resources.  </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceSchedule" type="em:ResourceScheduleType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Schedule information specific to arrivals, departures, availability, committed date, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceSpecialRequirementsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of any special needs related to the requested resource.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceStatusKeywordValue" type="em:ValueType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Any value from a discrete managed list, used to specify the general state of a resource if known.  where the content of valueListUrn is the Uniform Resource Name of a published list of values and definitions, and the content of value is a string denoting the value itself .  Multiple instances MAY occur.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceTracking" type="em:ResourceTrackingType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information that can indicate arrival , departure and locations of a resource pertaining to a route.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResponseCode" type="edxl:ResponseCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Used to accept or decline a Request, Response, Unsolicited Offer, or a Request Return.  Decline indicates the request, response or offer is not accepted.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResponseReasonCode" type="edxl:ResponseCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Explanation for a declined Request, Response, Unsolicited Offer, or a Request Return.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RouteLocation" type="nc:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A location along the delivery route for a resource.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SecurityStatusCode" type="have-codes:SecurityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Indicator of the status of security procedures in the hospital.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SecurityStatusCommentText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>One or more comments</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ServiceCoverageStatus" type="em:ServiceCoverageStatusType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Container element of all the elements of service coverage.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SpinalServiceCoverageStatusCode" type="have-codes:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of spinal surgical services.  </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="StaffingCode" type="have-codes:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Indicator of the status of staffing at the hospital.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SubCategoryBedCapacity" type="em:SubCategoryBedCapacityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The container element for the subcategories of each bed type.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SubCategoryBedCategoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The name of the sub-category bed type</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Surgery" type="em:SurgeryType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of general surgery services. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SurgeryGeneralServiceCoverageStatusCode" type="have-codes:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Indicator of the availability of  general surgery  services coverage.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TriageBlackQuantity" type="nc:QuantityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Number of deceased victims</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TriageGreenQuantity" type="nc:QuantityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Number of victims with minor needs</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TriageRedQuantity" type="nc:QuantityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Number of victims with immediate needs.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TriageYellowQuantity" type="nc:QuantityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Number of victims with delayed needs</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Value" type="em:ValueType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Contains a value list URN and value pair.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ValueListURN" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The name of the certified list maintained by the Community of Interest (COI) for the value referenced.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ValueText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A Value from a certified list maintained by the Community of Interest (COI) for the referenced element.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VascularServiceCoverageStatusCode" type="have-codes:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of vascular surgical services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
</xsd:schema>

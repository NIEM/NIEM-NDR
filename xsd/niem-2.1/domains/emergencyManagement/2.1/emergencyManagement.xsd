    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NotificationFunctionCategoryCode" type="em:NotificationFunctionCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The code  value denoting the nature of the notification message. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NotificationID" type="niem-xsd:string" nillable="true">

<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://niem.gov/niem/domains/emergencyManagement/2.1" version="1" xmlns:i2="http://niem.gov/niem/appinfo/2.1" xmlns:niem-xsd="http://niem.gov/niem/proxy/xsd/2.0" xmlns:i="http://niem.gov/niem/appinfo/2.0" xmlns:edxl="http://niem.gov/niem/edxl/2.1" xmlns:s="http://niem.gov/niem/structures/2.0" xmlns:iso_639-3="http://niem.gov/niem/iso_639-3/2.0" xmlns:have-codes="http://niem.gov/niem/have-codes/2.1" xmlns:em="http://niem.gov/niem/domains/emergencyManagement/2.1" xmlns:apco="http://niem.gov/niem/apco/2.1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:nc="http://niem.gov/niem/niem-core/2.0">
  <xsd:annotation>
    <xsd:documentation>Emergency Management</xsd:documentation>
    <xsd:appinfo>
      <i:ConformantIndicator>true</i:ConformantIndicator>
    </xsd:appinfo>
  </xsd:annotation>
  <xsd:import schemaLocation="../../../structures/2.0/structures.xsd" namespace="http://niem.gov/niem/structures/2.0"/>
  <xsd:import schemaLocation="../../../appinfo/2.0/appinfo.xsd" namespace="http://niem.gov/niem/appinfo/2.0"/>
  <xsd:import schemaLocation="../../../appinfo/2.1/appinfo.xsd" namespace="http://niem.gov/niem/appinfo/2.1"/>
  <xsd:import schemaLocation="../../../apco/2.1/apco.xsd" namespace="http://niem.gov/niem/apco/2.1"/>
  <xsd:import schemaLocation="../../../edxl/2.1/edxl.xsd" namespace="http://niem.gov/niem/edxl/2.1"/>
  <xsd:import schemaLocation="../../../have-codes/2.1/have-codes.xsd" namespace="http://niem.gov/niem/have-codes/2.1"/>
  <xsd:import schemaLocation="../../../iso_639-3/2.0/iso_639-3.xsd" namespace="http://niem.gov/niem/iso_639-3/2.0"/>
  <xsd:import schemaLocation="../../../niem-core/2.0/niem-core.xsd" namespace="http://niem.gov/niem/niem-core/2.0"/>
  <xsd:import schemaLocation="../../../proxy/xsd/2.0/xsd.xsd" namespace="http://niem.gov/niem/proxy/xsd/2.0"/>
  <xsd:simpleType name="AlarmAudibleDescriptionCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type code indicating whether the alarm is audible or silent.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Audible">
        <xsd:annotation>
          <xsd:documentation>The alarm is making an audible sound.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Silent">
        <xsd:annotation>
          <xsd:documentation>The alarm is silent and not making any sound.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="AlarmAudibleDescriptionCodeType">
    <xsd:annotation>
      <xsd:documentation>A data type code indicating whether the alarm is audible or silent.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="em:AlarmAudibleDescriptionCodeSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="AlarmConfirmationType">
    <xsd:annotation>
      <xsd:documentation>The data type for the container for information related to the confirmation of an alarm.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="em:AlarmConfirmationMethodDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmConfirmationCallPrivacyBypassPasscodeText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmConfirmationCallToPremisesIndicator" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmConfirmationCallToPremisesResultsDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="AlarmEventLocationAugmentationType">
    <xsd:annotation>
      <xsd:documentation>A data type for additional information about a location.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="AugmentationType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:AugmentationType">
        <xsd:sequence>
          <xsd:element ref="em:LocationDirectionsText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:LocationInformationText" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="AlarmEventType">
    <xsd:annotation>
      <xsd:documentation>A data type for an alarm event.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ActivityType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:ActivityType">
        <xsd:sequence>
          <xsd:element ref="em:AlarmEventCategory" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmEventCallBackTelephoneNumber" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmEventLocationCategory" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmAudibleIndicator" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmEventDetailsText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmEventRequestorReceiveDate" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmEventCallPrivacyBypassCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmEventCallToPremisesIndicator" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmEventPermit" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmEventResponseAction" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmEventDispatchAgency" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="AlarmLocationAugmentationType">
    <xsd:annotation>
      <xsd:documentation>A data type for the additional information related to the location of the alarm.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="AugmentationType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:AugmentationType">
        <xsd:sequence>
          <xsd:element ref="em:AlarmLocationDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmLocationCategoryText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmLocationDirectionText" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="AlarmLocationType">
    <xsd:annotation>
      <xsd:documentation>A data type for an applied augmentation for type nc:LocationType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="LocationType"/>
        <i2:AugmentedTypeIndicator>true</i2:AugmentedTypeIndicator>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:LocationType">
        <xsd:sequence>
          <xsd:element ref="em:AlarmLocationAugmentation" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="AlarmPermitType">
    <xsd:annotation>
      <xsd:documentation>A data type for a permit for the alarm event site.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="em:PermitCategoryText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:PermitIdentification" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="AlarmType">
    <xsd:annotation>
      <xsd:documentation>A data type for an alarm.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="em:AlarmAudibleIndicator" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmAudibleDescriptionCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmCallBackTelephoneNumber" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmCategory" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmDetailsText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmDispatchActivity" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmDispatchAgency" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmMonitoringServiceProvider" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmMonitoringServiceProviderReceiveDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:AlarmRequestedResponseActionDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="AlertAffectedLocationAugmentationType">
    <xsd:annotation>
      <xsd:documentation>A data type for an alert affected location augmentation.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="AugmentationType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:AugmentationType">
        <xsd:sequence>
          <xsd:element ref="nc:AreaPolygonGeographicCoordinate" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="nc:LocationAltitudeMeasure" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="em:Ceiling" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="AlertAffectedLocationType">
    <xsd:annotation>
      <xsd:documentation>A data type for an applied augmentation for type nc:LocationType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="LocationType"/>
        <i2:AugmentedTypeIndicator>true</i2:AugmentedTypeIndicator>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:LocationType">
        <xsd:sequence>
          <xsd:element ref="em:AlertAffectedLocationAugmentation" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="AlertCategoryCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for a code set denoting the type of alert message.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Ack">
        <xsd:annotation>
          <xsd:documentation>Acknowledgment of receipt of an earlier message</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Response">
        <xsd:annotation>
          <xsd:documentation>A response to a previous request</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SensorConfiguration">
        <xsd:annotation>
          <xsd:documentation>These messages are for reporting configuration during power up or after Installation or Maintenance.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Dispatch">
        <xsd:annotation>
          <xsd:documentation>A commitment of resources or assistance</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Report">
        <xsd:annotation>
          <xsd:documentation>New information regarding an incident or activity</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Error">
        <xsd:annotation>
          <xsd:documentation>Rejection of an earlier message (for technical reasons)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Request">
        <xsd:annotation>
          <xsd:documentation>A request for resources, information or action</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SensorDetection">
        <xsd:annotation>
          <xsd:documentation>These are high priority messages which report sensor detections.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SensorControl">
        <xsd:annotation>
          <xsd:documentation>These are messages used to control sensors/sensor concentrator components behavior.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SensorStatus">
        <xsd:annotation>
          <xsd:documentation>These are concise messages which report sensors/sensor concentrator component status or state of health.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Cancel">
        <xsd:annotation>
          <xsd:documentation>A cancellation or revocation of a previous message</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Update">
        <xsd:annotation>
          <xsd:documentation>Updated information superseding a previous message</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="AlertCategoryCodeType">
    <xsd:annotation>
      <xsd:documentation>A data type for a code set denoting the type of alert message.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="em:AlertCategoryCodeSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="AlertEventDetailsCategoryCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for the category code designated to an alert event.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Security">
        <xsd:annotation>
          <xsd:documentation>Law enforcement, military, homeland and local/private security</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Transport">
        <xsd:annotation>
          <xsd:documentation>Public and private transportation</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Health">
        <xsd:annotation>
          <xsd:documentation>Medical and public health</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Rescue">
        <xsd:annotation>
          <xsd:documentation>Rescue and Recovery</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CBRNE">
        <xsd:annotation>
          <xsd:documentation>Chemical, Biological, Radiological, Nuclear, or High Yield Explosive threat or attack</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Other">
        <xsd:annotation>
          <xsd:documentation>Other events</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Infra">
        <xsd:annotation>
          <xsd:documentation>Utility, telecommunication, other non-transport infrastructure</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Geo">
        <xsd:annotation>
          <xsd:documentation>Geophysical (inc. landslide)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Fire">
        <xsd:annotation>
          <xsd:documentation>Fire suppression and rescue</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Safety">
        <xsd:annotation>
          <xsd:documentation>General emergency and public safety</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Met">
        <xsd:annotation>
          <xsd:documentation>Meteorological (inc. flood)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Env">
        <xsd:annotation>
          <xsd:documentation>Pollution and other environmental</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="AlertEventDetailsCategoryCodeType">
    <xsd:annotation>
      <xsd:documentation>A data type for the category code designated to an alert event.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="em:AlertEventDetailsCategoryCodeSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="AlertEventDetailsCertaintyCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for the certainty code designated to an alert event.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Possible">
        <xsd:annotation>
          <xsd:documentation>Possible but not likely (less than or equal to 50%)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Observed">
        <xsd:annotation>
          <xsd:documentation>Determined to have occurred or to be ongoing</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Unknown">
        <xsd:annotation>
          <xsd:documentation>Certainty unknown</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Likely">
        <xsd:annotation>
          <xsd:documentation>Likely greater than ~50%</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Unlikely">
        <xsd:annotation>
          <xsd:documentation>Not expected to occur (~0)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="AlertEventDetailsCertaintyCodeType">
    <xsd:annotation>
      <xsd:documentation>A data type for the certainty code designated to an alert event.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="em:AlertEventDetailsCertaintyCodeSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="AlertEventDetailsResponseRecommendationCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for the response recommendation code designated to an alert event.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Monitor">
        <xsd:annotation>
          <xsd:documentation>Attend to information sources as described in instruction</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Evacuate">
        <xsd:annotation>
          <xsd:documentation>Relocate as instructed in the instruction</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Assess">
        <xsd:annotation>
          <xsd:documentation>Evaluate the information in this message </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="None">
        <xsd:annotation>
          <xsd:documentation>No action recommended</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Execute">
        <xsd:annotation>
          <xsd:documentation>Execute a pre-planned activity identified in instruction</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Shelter">
        <xsd:annotation>
          <xsd:documentation>Take shelter in place or per instruction</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Prepare">
        <xsd:annotation>
          <xsd:documentation>Make preparations per instruction</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="AlertEventDetailsResponseRecommendationCodeType">
    <xsd:annotation>
      <xsd:documentation>A data type for the response recommendation code designated to an alert event.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="em:AlertEventDetailsResponseRecommendationCodeSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="AlertEventDetailsSeverityCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for the severity code designated to an alert event.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Moderate">
        <xsd:annotation>
          <xsd:documentation>Possible threat to life or property</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Unknown">
        <xsd:annotation>
          <xsd:documentation>Severity unknown</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Extreme">
        <xsd:annotation>
          <xsd:documentation>Extraordinary threat to life or property</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Minor">
        <xsd:annotation>
          <xsd:documentation>Minimal threat to life or property</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Severe">
        <xsd:annotation>
          <xsd:documentation>Significant threat to life or property</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="AlertEventDetailsSeverityCodeType">
    <xsd:annotation>
      <xsd:documentation>A data type for the severity code designated to an alert event.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="em:AlertEventDetailsSeverityCodeSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="AlertEventDetailsType">
    <xsd:annotation>
      <xsd:documentation>A data type for an alert event details.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="em:AlertEventDetailsAudienceDescriptionText" minOccurs="0" maxOccurs="unbounded"/>

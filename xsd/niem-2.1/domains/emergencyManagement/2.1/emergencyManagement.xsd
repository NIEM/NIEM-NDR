          <xsd:documentation>Urgency unknown</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Expected">
        <xsd:annotation>
          <xsd:documentation>Responsive action should be taken soon (within the hour)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Future">
        <xsd:annotation>
          <xsd:documentation>Responsive action should be taken in the near future</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Past">
        <xsd:annotation>
          <xsd:documentation>Responsive action is no longer required</xsd:documentation>

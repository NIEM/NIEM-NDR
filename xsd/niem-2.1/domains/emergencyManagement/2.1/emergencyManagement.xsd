      <xsd:appinfo>
        <i:Base i:name="NotificationRecipient"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="em:NotificationRecipient" name="NotificationRecipientIdentification" type="nc:IdentificationType" nillable="true">

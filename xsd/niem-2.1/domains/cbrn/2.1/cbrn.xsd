    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RequestAgencyCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="nc:OrganizationName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="nc:OrganizationIdentification" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="RequestDataType">
    <xsd:annotation>
      <xsd:documentation>A data type to provide metadata about a request.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RequestID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RequestDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RequestScopeCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ActivityName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:UserName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RequestAgency" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ResponseReportType">
    <xsd:annotation>
      <xsd:documentation>A data type for a report provided in response to a request message.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="ReportType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="cbrn:ReportType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RequestData" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:CaseResponseCode" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ScanIdentifierDataType">
    <xsd:annotation>
      <xsd:documentation>A data type that provides identification data about a scan.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:TraversalID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DetectionEventUUID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MeasuredItemID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MeasuredItemIDKindCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MeasuredItemKindCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ScanQualityCode" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SecondaryInspectionReferralType">
    <xsd:annotation>
      <xsd:documentation>A data type for information regarding a secondary inspection referral.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="InspectionDataType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="cbrn:InspectionDataType">
        <xsd:sequence>
          <xsd:element ref="cbrn:SecondaryInspectionReferralID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:SecondaryInspectionReferralReasonCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:SecondaryInspectionKindCode" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SecondaryInspectionResolutionType">
    <xsd:annotation>
      <xsd:documentation>A data type for information regarding the resolution of a secondary inspection of an item of interest.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="InspectionDataType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="cbrn:InspectionDataType">
        <xsd:sequence>
          <xsd:element ref="cbrn:SecondaryInspectionReferralID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:InspectionThreatFindingCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:NuclideCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:SourceSystemFindingCategoryText" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ShieldingType">
    <xsd:annotation>
      <xsd:documentation>A data type for describing the shielding that has been applied to an item.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MeasuredItemID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:LayerQuantity" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:Layer" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RadEncounterDeviceToShieldingDistance" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SiteLocationType">
    <xsd:annotation>
      <xsd:documentation>A data type to provide information about the location of a site.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="nc:LocationName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:GeoPosition" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:AddressDetails" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:SiteSpecialInfoText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:LocationDirectionsText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:SiteMobileIndicator" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:SiteMobileDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MapGuideLocation" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SpatialOrientationMeasureType">
    <xsd:annotation>
      <xsd:documentation>A data type for the spatial orientation of an item with respect to True North, the horizontal axis, and the vertical axis; i.e., pitch, yaw, and roll.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:TrueNorthOffset" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:VerticalOffset" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RotationalOffset" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="cbrn:dataQualityCode" use="optional"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SpectrumChannelDataType">
    <xsd:annotation>
      <xsd:documentation>A data type that provides the spectrum measurement values as counts by channel for the entire spectrum or a region of interest.  The first channel number shall always be 0.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:CountValues" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="cbrn:channelQuantity" use="optional"/>
        <xsd:attribute ref="cbrn:compressionCode" use="optional"/>
        <xsd:attribute ref="cbrn:listModeCode" use="optional"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SpectrumDataType">
    <xsd:annotation>
      <xsd:documentation>A data type that provides measurement data from a spectroscopic-capable encounter device.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:SpectrumID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RefBackgroundSpectrumID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RefCalibrationSpectrumID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DetectorDesignatorText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DetectorKindText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:LiveTimeDuration" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DwellDuration" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ElapsedSweepsQuantity" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EnergyCalibrationAdjustCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:NonLinearityCorrection" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RadSourceKindCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:SpectrumOverflowValue" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:SpectrumChannelData" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="cbrn:calibrationMeasurementUUID" use="optional"/>
        <xsd:attribute ref="cbrn:dataQualityCode" use="optional"/>
        <xsd:attribute ref="cbrn:sampleNumber" use="optional"/>
        <xsd:attribute ref="cbrn:spectrumKindCode" use="optional"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="StringListSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for a  white space-delimited list of string.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:list itemType="xsd:string"/>
  </xsd:simpleType>
  <xsd:complexType name="StringListType">
    <xsd:annotation>
      <xsd:documentation>A data type for a  white space-delimited list of string.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="cbrn:StringListSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="SystemEventType">
    <xsd:annotation>
      <xsd:documentation>A data type for basic data elements of a system event for audit reporting purposes.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:SystemEventDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:UserName" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="cbrn:systemOperatingModeCode" use="optional"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TemplateRankedNuclideType">
    <xsd:annotation>
      <xsd:documentation>A data type for reporting a Nuclide considered but not selected as a correct identification.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:NuclideCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:NuclideIdConfidence" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TemplateRankingDataType">
    <xsd:annotation>
      <xsd:documentation>A data type to provide ranking of nuclides from a template library that have been identified as candidates for the correct nuclide(s) in a spectrum, and their confidence of correct identification. The list of nuclides should be in descending order of Z number.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:TemplateLibraryName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:TemplateRankedNuclide" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ThreeDimensionalGeographicCoordinateType">
    <xsd:annotation>
      <xsd:documentation>A data type for a location identified by latitude, longitude, and elevation.  All coordinate values are in the form of +/-dd:mm:ss where dd is restricted to 00 (inclusive) to 90 (exclusive), and mm and ss are restricted to 00 (inclusive) to 60 (exclusive).</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="TwoDimensionalGeographicCoordinateType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:TwoDimensionalGeographicCoordinateType">
        <xsd:sequence>
          <xsd:element ref="cbrn:CoordinatesMethodCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:GeographicCoordinates" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:CoordinateDuration" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="cbrn:dataQualityCode" use="optional"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TraversalType">
    <xsd:annotation>
      <xsd:documentation>A data type for the end-to-end workflow of a conveyance through an encounter process. One or more detection events occur during a traversal.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ActivityType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:ActivityType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:TraversalID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:TraversalBeginDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:TraversalEndDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:TraversalOperatingModeCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DetectionEventUUID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ConveyanceID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ConveyanceBoundForCode" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VelocityMeasureType">
    <xsd:annotation>
      <xsd:documentation>A data type for a velocity measurement.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="MeasureType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:MeasureType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:VelocityUnitCode" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:attribute name="algorithmTechnicalVersionText" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation>The version identifier of the technical specification of the algorithm that was used in a physical implementation.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="calibrationKindCode" type="cbrncl:CalibrationKindCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A kind of calibration (e.g., Energy, FWHM, ...).</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="calibrationMeasurementUUID" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation>The UUID of the measurement from which a calibration was derived.  UUID is as defined in SO/IEC 11578.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="cepValue" type="xsd:decimal">
    <xsd:annotation>
      <xsd:documentation>Circular error probability of a positional location. This is a radius in feet of a circle within which the true position is located with a probability is .50.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="channelQuantity" type="xsd:positiveInteger">
    <xsd:annotation>
      <xsd:documentation>The total number of channel count values contained in the CountValue element.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="compressionCode" type="cbrncl:CompressionCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>The algorithm (if any) by which data has been compressed.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="convergenceDuration" type="xsd:duration">
    <xsd:annotation>
      <xsd:documentation>The time (in ISO 8601 format) it took the analysis algorithm to converge to produce the alarm.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="dataFormatCode" type="cbrncl:DataFormatCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>Identifies a data format specification that has been used to structure an output data file from a device.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="dataQualityCode" type="cbrncl:DataQualityCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A qualitative assessment of the validity of the data.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="detectionEventCategoryCode" type="cbrncl:DetectionEventCategoryCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A classification of a Detection Event.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="detectionEventKindCode" type="cbrncl:DetectionEventKindCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A kind of Detection Event.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="endEnergyValue" type="xsd:double">
    <xsd:annotation>
      <xsd:documentation>The ending energy of the window that the data represents.  If omitted, the window ends at the highest energy that can be collected by the encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="listModeCode" type="cbrncl:ChannelDataListModeCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>The kind of channel data or the time precision of the list mode.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="nuclideActivityUnitsCode" type="cbrncl:NuclideActivityUnitsCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>Unit of measure in which a nuclide activity value is expressed.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="nuclideBetaEndPointEnergyValue" type="xsd:positiveInteger">
    <xsd:annotation>
      <xsd:documentation>Maximum beta particle energy approximation.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="occupiedIndicator" type="xsd:boolean">
    <xsd:annotation>
      <xsd:documentation>True if the encounter device was occupied when a measurement was made; i.e., the measurement was taken on a real item seeking to determine if a radiological source was present.  False otherwise.  If the encounter device does not have occupancy sensor(s), this element should not be used.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="parameterSetID" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation>Locally-scoped identifier referencing the parameter set used with an analysis algorithm.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="parameterUnitsText" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation>A Unit of Measure that qualifies the parameter value.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="radAbsorbedDoseRateUnitsCode" type="cbrncl:RadAbsorbedDoseRateUnitsCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>Unit of measure in which radiation absorbed dose rate is expressed.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="radAbsorbedDoseUnitsCode" type="cbrncl:RadAbsorbedDoseUnitsCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>Unit of measure for radiation absorbed dose.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="radCountRateUnitsCode" type="cbrncl:RadCountRateUnitsCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>Unit of measure for a radiation count rate value.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="radDoseRateUnitsCode" type="cbrncl:RadDoseRateUnitsCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>Unit of measure in which radiation dose is expressed.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="radDoseUnitsCode" type="cbrncl:RadDoseUnitsCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>Unit of measure for radiation dose.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="radEncounterDeviceModeCode" type="cbrncl:RadEncounterDeviceModeCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>The operating mode of an encounter device. Note: does not apply to subassemblies/components of a device.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="radEncounterDeviceOperatingCode" type="cbrncl:RadEncounterDeviceOperatingCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>Identifies the operational state of the device; i.e., enabled for live data collection/measurements or in a "simulated data" state. Note: does not apply to subassemblies/components of a device.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="radEnergyUnitsCode" type="cbrncl:RadEnergyUnitsCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>Unit of measure for a radiation energy value.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="radExposureRateUnitsCode" type="cbrncl:RadExposureRateUnitsCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>Unit of measure for radiation exposure rate.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="radExposureUnitsCode" type="cbrncl:RadExposureUnitsCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>Unit of measure for radiation exposure.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="radFWHMUnitsCode" type="cbrncl:RadFWHMUnitsCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>Unit of measure in which photopeak radiation (FWHM) is expressed.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="sampleNumber" type="xsd:positiveInteger">
    <xsd:annotation>
      <xsd:documentation>The sample number (starting from 1) of this spectrum (used only when the spectrum is contained within a SpectrumMeasurement element).</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="sequenceNumber" type="xsd:positiveInteger">
    <xsd:annotation>
      <xsd:documentation>Identifies the sequence, or placement, of a file in a fileset, if the fileset is ordered.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="spectrumKindCode" type="cbrncl:SpectrumKindCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>Identifies whether the spectrum kind is PHA or MCS. If not reported, PHA is default value.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="startEnergyValue" type="xsd:double">
    <xsd:annotation>
      <xsd:documentation>The starting energy of the window that the data represents.  If omitted, the window starts at the lowest energy that can be collected by the encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="subEquationNumber" type="xsd:positiveInteger">
    <xsd:annotation>
      <xsd:documentation>Index of the subequation to which a set of coefficients applies.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="systemOperatingModeCode" type="cbrncl:RadEncounterDeviceOperatingCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>Operating mode of the system at the time of a system event.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:element name="AcknowledgementData" type="cbrn:AcknowledgementDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information regarding an acknowledgement.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AcknowledgementDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date/time an acknowledgement was generated.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AcknowledgementID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of an acknowledgement.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AcknowledgingActivityName" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Name of the activity that produced an acknowledgement. This property can be used to provide data for workflow coordination by the sending or receiving systems.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AcknowledgingAgency" type="cbrn:AcknowledgingAgencyType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An organization that is responsible for generating an acknowledgement message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AcknowledgingAgencyCode" type="cbrncl:GNDAOrgCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Organization that is responsible for generating an acknowledgement.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ActivityName" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Name of the activity associated with a request. This property can be used to provide data for workflow coordination by the sending or receiving systems.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:UserName" name="ActivityUserLogonID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier for a person in the form of a person's system access (logon) id.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="UserName"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AddressDetails" type="nc:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Detailed address of a location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmAcknowledgementCode" type="cbrncl:AlarmAcknowledgementCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of a kind of alarm acknowledgement.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmAudibleIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the alarm was annunciated by sound.  False otherwise. This element should not be used if the device does not have an audible alarm capability.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>DateTime the alarm was recorded or cancelled.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmDescriptionCode" type="cbrncl:AlarmDescriptionCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the nature of the threat annunciated by an alarm.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Text description of the nature of the threat annunciated by an alarm.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmDuration" type="niem-xsd:duration" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Duration of the alarm in ISO  8601 format.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlarmID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of a set of alarm data. There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlgorithmCreatorName" type="nc:ProperNameTextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Name of the organization that created an algorithm.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlgorithmDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of an algorithm.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlgorithmImplementationVersionID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>External identifier of the physical implementation version of an algorithm.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlgorithmName" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Name of an algorithm.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlgorithmParameterSet" type="cbrn:ParameterSetType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides sets of parameters and values used with analysis algorithms to assess the data collected during a Detection Event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlgorithmVersion" type="cbrn:AlgorithmVersionType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Version of an algorithm.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlgorithmVersionDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of an algorithm version</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AlgorithmVersionReleaseDate" type="niem-xsd:date" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The release date of an algorithm version implementation.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AnalysisAlgorithm" type="cbrn:AlgorithmType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Algorithm used in the analysis of the encounter device data to produce an analysis results.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AnalysisEndDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date/time when analysis of a detection event ended.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AnalysisOrgCode" type="cbrncl:GNDAOrgCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Organization that performed a detection event analysis.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AnalysisStartDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date/time when analysis of a detection event started.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ArealDensityValue" type="niem-xsd:decimal" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The areal density of the aggregate set of shielding layers expressed in unit of measure g/cm2.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ArrayXY" type="cbrn:ArrayXYType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Calibration data as ordinal pairs.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AtomicNumberValue" type="cbrn:PositiveIntegerType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The estimated atomic number of the aggregate shielding layers.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AzimuthOffset" type="cbrn:DecimalListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The azimuth offset angle from an expected bearing to the actual bearing. Angular measurement is in degrees minutes seconds. A clockwise angle is positive and counterclockwise is negative.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="BackgroundCountValues" type="cbrn:DoubleListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A white space delimited list of background count values; can be one value for all samples, or a unique value for each sample.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:SpectrumID" name="BackgroundSpectrumID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier for a spectrum measurement, where RadSourceKindCode is Background.  There is no required format for the ID value.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="SpectrumID"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="BackgroundSubtractionIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if a background spectrum was subtracted from the raw item spectrum to produce the processed spectrum data, false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:BinaryObject" name="BinaryUTF8Object" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A binary encoding of data in UTF8 form.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="BinaryObject"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="BooleanList" type="cbrn:BooleanListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A white space-delimited list of boolean values.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CBRNECase" type="cbrn:CBRNECaseType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An aggregation of information about activities and events associated with detection and interdiction of CBRNE threats.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CBRNEncounterDevice" type="cbrn:CBRNEncounterDeviceType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides information on a device used to perform measurements or collect data during a CBRN Detection Event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CalibrationCreationDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date/time of a calibration's creation (ISO 8601 UTC format)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CalibrationID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Locally scoped identifier of a calibration.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CalibrationNuclide" type="cbrn:CalibrationNuclideType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identification of a nuclide source used in a calibration.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:SpectrumID" name="CalibrationSpectrumID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of a calibration spectrum. Applies only if the RadSourceKindCode for the spectrum is Calibration-baseline or Calibration-update. A spectrum that refers to a CalibrationSpectrumID must have a RadSourceKindCode of Item or Other.  There is no required format for the ID value.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="SpectrumID"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CargoDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the contents of a vehicle, container, or other conveyance.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseClosedIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if a case is closed; false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseEventDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date and time of the first detection event associated with the case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseKindCode" type="cbrncl:CaseKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A kind of case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseLocationDescription" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the locale or location associated with a case when it was initiated. For a case that is a collection of cases, may describe a route or involved locations/locales.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseMetadata" type="cbrn:DataFileMetadataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Metadata information about a case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseOfInterestUUID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The identifier of a case of interest  to which another case is related.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CasePriorityCode" type="cbrncl:CasePriorityCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A priority of a case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseRelationshipDescriptionText" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the nature, reason, status, etc of the relationship.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseRelationshipKindCode" type="cbrncl:CaseRelationshipKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The kind of relationship between two cases.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseRequestCode" type="cbrncl:CaseRequestCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of a kind of Case request.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseResponseCode" type="cbrncl:CaseResponseCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A code identifying the nature of the ability to provide the requested data, or the method of providing the response.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseSet" type="cbrn:CaseSetType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A set of cases that are related in some manner.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseSetQuantity" type="niem-xsd:nonNegativeInteger" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The quantity of cases contained in a Case, or otherwise related to a Case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseStartDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date and time a case was initiated.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseStatus" type="cbrn:CaseStatusType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information on the status of a case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseStatusCode" type="cbrncl:CaseStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A status of a case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseStatusDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>DateTime a status was reported.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseStatusIssuerCode" type="cbrncl:GNDAOrgCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Organization reporting a case status.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseThreatLevelCode" type="cbrncl:CaseThreatLevelCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The threat level represented by the activities or items represented by the case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseUUID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A universally unique identifier for a case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseWindowEndDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>DateTime for the time window end for a BOLO kind of case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CaseWindowStartDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>DateTime for the time window start for a BOLO kind of case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ClassificationReasonText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Reason for a classification, with reference to the applicable classification guide.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ClassifiedByText" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the classification authority.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConfidenceAssessment" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for the confidence that an identification is correct.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:ConfidenceAssessment" name="ConfidenceCode" type="cbrncl:ConfidenceCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A confidence value expressed as a descriptive adjective.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="ConfidenceAssessment"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:ConfidenceAssessment" name="ConfidencePointValue" type="nc:MeasurePointValueType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A specific confidence value.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="ConfidenceAssessment"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:ConfidenceAssessment" name="ConfidenceRangeValue" type="nc:MeasureRangeValueType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The minimum and maximum value range in which a ConfidencePointValue is expressed.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="ConfidenceAssessment"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:Conveyance" name="Conveyance" type="cbrn:ConveyanceType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:ConveyanceType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="Conveyance"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceAugmentation" type="cbrn:ConveyanceAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Information describing a tangible item that is used to transport goods and/or persons from one location to another location; e.g., ship, airplane, vehicle, train, pipeline.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ConveyanceType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceBoundForCode" type="cbrncl:ConveyanceBoundForCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of where a conveyance is bound for when it completes a traversal.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceConveyanceRelationship" type="cbrn:ConveyanceConveyanceRelationshipType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of a relationship  between two conveyances.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceID" type="cbrn:ConveyanceIDReferenceType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier for a conveyance processed through a traversal.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceIdentification" type="cbrn:ConveyanceIDReferenceType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identification of the conveyance that is associated with an organization.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceKindCode" type="cbrncl:ConveyanceKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of the type of the conveyance.  For example: Ship, Airplane, Truck, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceOrgRelationship" type="cbrn:ConveyanceOrgRelationshipType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of a relationship  between a conveyance and an organization.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceOrgRelationshipKindCode" type="cbrncl:ConveyanceOrgRelationshipKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the kind of relationship between the conveyance and organization.  For example, an aircraft may have an owner, operator, leasee, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyancePrimaryColorCode" type="cbrncl:ConveyanceColorCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Primary color of a container.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="nc:ConveyanceRegistration" name="ConveyanceRegistration" type="cbrn:ConveyanceRegistrationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Applied augmentation for type nc:ConveyanceRegistrationType</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ConveyanceRegistration"/>
        <i2:AugmentedElementIndicator>true</i2:AugmentedElementIndicator>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceRegistrationAugmentation" type="cbrn:ConveyanceRegistrationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>Data from the official registration of a conveyance with an authority.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ConveyanceRegistrationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceRelationshipKindCode" type="cbrncl:ConveyanceRelationshipKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Kind of conveyance relationship; for example, contained in/on, or connected to.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceRelationshipOriginID" type="cbrn:ConveyanceIDReferenceType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of the conveyance that is the start/origin of the relationship.  By convention, the start of the relationship is the conveyee and the end of the relationship is the conveyor; for example a trailer is conveyed by a tractor, or a container is conveyed by a vessel or a trailer.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceRelationshipTargetID" type="cbrn:ConveyanceIDReferenceType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of the conveyance that is the end/target of the relationship.  By convention, the end of the relationship is the conveyor and the start of the relationship is the conveyee.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceSecondaryColorCode" type="cbrncl:ConveyanceColorCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The secondary color of a conveyance.  Identifies a lower-most or rear-most color of a two-tone conveyance or a lesser color of a multi-colored conveyance.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceSequenceNumber" type="cbrn:PositiveIntegerType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifies the position of the conveyee if there is more than one associated with the same conveyor during the same period of time. For example, for a truck consisting of a tractor with two trailers, the first trailer would have a sequence number of one.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ConveyanceWeightDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>If there are multiple ways to describe conveyance weight, such as gross weight, axle weight, etc., this element is used to provide the description that is appliable to the value provided by ConveyanceWeightMeasure.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CoordinateDuration" type="niem-xsd:duration" nillable="true">
    <xsd:annotation>
      <xsd:documentation>If georef coordinates have been calculated based on a set of coordinate readings, this is the duration of time over which the readings were made (ISO 8601 format).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:GeographicCoordinates" name="CoordinateList" type="cbrn:StringListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Geographical coordinates as a list of up to 5 character string values providing dateTime of the coordinates reading, latitude, longitude, and elevation. DateTime is in ISO 8601 format.  Latitude and longitude are in decimal degrees.  Elevation is an optional fourth value in the list, and is the height above sea level of the point on earth given by the latitude and longitude. The fifth string in the list provides the units of measure for elevation.  If not included but an elevation value is given, then units are in meters.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="GeographicCoordinates"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:GeographicCoordinates" name="CoordinateSet" type="cbrn:GeographicCoordinatesType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Geographical coordinates as individually tagged coordinate values.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="GeographicCoordinates"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CoordinatesDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date/time of coordinates measurement (ISO 8601 UTC format).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CoordinatesMethodCode" type="cbrncl:CoordinatesMethodCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The method by which the coordinates were determined.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CountDoseData" type="cbrn:CountDoseDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The measurement data from a count dose-capable encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CountValues" type="cbrn:DoubleListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A set of spectra counts as a white space-delimited list of positive integers. The counts begin at channel 0. Any channel for which data is missing will record a count of zero.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CredentialsAuthenticatedCode" type="cbrncl:CredentialsAuthenticatedCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Credentials authentication verification.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CurrentVersionID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of the current version. There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DataFileCaptureData" type="cbrn:DataFileCaptureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information regarding the means and methods of recording the data in a digital file.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DataFileCompressionDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the compression method applied to a file's contents, if any.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DataFileDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A text description of the subject matter recorded in a digital data file.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DataFileMetadata" type="cbrn:DataFileMetadataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides information about a digital data file.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DataFileName" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Name of a digital data file.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DataFileSequence" type="cbrn:DataFileSequenceType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides information on accessible subsets of data in a file, if any.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DataFileSequenceDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the contents of a subset of a digital data file.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DataFileSequenceIdentifier" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of the sequence of a data subset in a file that contains sequences of recorded digital data.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DataFileSubjectCode" type="cbrncl:BinarySubjectCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the kind of subject matter recorded in a digital data file. If the kind is Other, a description should be provided in BinaryDescriptionText.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DataFileUUID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Universally unique identifier of a digital data file.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeclassDate" type="niem-xsd:date" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Specific date at which the applicable information is automatically declassified.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeclassEventText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of an event that triggers declassification.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeclassExceptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of any exceptions to automatic declassification.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeclassManualReviewIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if a manual review is required for declassification; false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEvent" type="cbrn:DetectionEventType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The set of all data collected during an Event that involves the inspection of an Item(s) for the purpose of detecting the presence of illicit goods and materials. This includes data collected by the device(s) used to perform the detection as well as information input by operator(s) involved in the detection activities that the event involves. A detection event can occur in many venues, such as a CBP Port of Entry, a USCG boarding, a state operated weigh station on a US highway, and a general aviation inspection at an airport in Mexico or Canada.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEventAlarmData" type="cbrn:DetectionEventAlarmDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides data on alarm(s) resulting from analysis of the data produced by an encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEventAnalysisResult" type="cbrn:DetectionEventAnalysisResultsType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides the results of analysis of the data produced by an encounter device.  Note: there may be multiple analyses of the same data based on applying different analysis methods/algorithms.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEventAnalyzedMeasurementData" type="cbrn:DetectionEventAnalyzedMeasurementDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifies the processed measurement data on which an assessment was performed.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEventAssessmentData" type="cbrn:DetectionEventAssessmentType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides assessment information produced by analysis of data collected during a Detection Event. This data may be produced by automated analysis by the device if it has this capability, or human operator input.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEventAssessmentID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An identifier for a detection event assessment.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEventDataFile" type="cbrn:DataFileType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides reference data for a digital file that contains device output, multimediam or other kinds data relevant to a Detection Event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEventDeviceCalibrationData" type="cbrn:RadEncounterDeviceCalibrationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides calibration data for a device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEventDeviceData" type="cbrn:DetectionEventDeviceDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides the data and references to the related data files collected by the device(s) that are used during a Detection Event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEventDeviceMeasurementData" type="cbrn:RadEncounterMeasurementType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides the data from the device(s) used during a Detection Event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEventID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An identifier for a Detection Event applied by the site that performs the inspection activities of the Detection  Event. There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEventLocation" type="nc:ProperNameTextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The physical location where a detection event occurred. This may be a placename or other descriptor that is meaningful and human-intelligible.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEventSiteID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of the site at which a detection event occurred. Typically this identifier is specific to the organization operating the site.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEventUUID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A universally unique identification for a Detection Event. UUID is as defined in SO/IEC 11578.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectionEventUserEntryData" type="cbrn:DetectionEventUserEntryDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides user-operator data entries relevant to a Detection Event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectorDesignatorText" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the detector used to produce a measurement.  Must use naming convention defined in the relevant specification document.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DetectorKindText" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identification of the detector that collected the measurement. Must use naming convention defined in the relevant specification document.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeviceDeviceRelationship" type="cbrn:DeviceDeviceRelationshipType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of a relationship  between two devices.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeviceIdentifierData" type="cbrn:DeviceIdentifierType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identification information for a device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeviceInfoDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>DateTime of the device identifier data.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeviceOperatorComments" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Comments entered by a device operator.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeviceOperatorID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of the encounter device operator.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeviceParameter" type="cbrn:DeviceParameterType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides the readable and settable parameters of an encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:DeviceParameterValue" name="DeviceParameterMaxValue" type="niem-xsd:decimal" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The maximum valid value to which a parameter may be set.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="DeviceParameterValue"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:DeviceParameterValue" name="DeviceParameterMinValue" type="niem-xsd:decimal" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The minimum valid value to which a parameter may be set.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="DeviceParameterValue"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeviceParameterName" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Standardized name for a device parameter.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:DeviceParameterValue" name="DeviceParameterPointValue" type="niem-xsd:decimal" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A parameter value where the value kind is decimal.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="DeviceParameterValue"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeviceParameterSettableIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the device can be commanded to set/change the parameter's value; false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeviceParameterStatusIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the device can be commanded to report the parameter's current value; false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeviceParameterUOMText" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The unit of measure (UOM) that applies to the value of a Device parameter, if a UOM is required.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeviceParameterValue" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for the value of a parameter.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:DeviceParameterValue" name="DeviceParameterValueIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A parameter value where the value kind is boolean.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="DeviceParameterValue"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:DeviceParameterValue" name="DeviceParameterValueListName" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The name of the enumeration list that contains a parameter's valid values.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="DeviceParameterValue"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:DeviceParameterValue" name="DeviceParameterValueListText" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A parameter value where the value kind is List.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="DeviceParameterValue"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:DeviceParameterValue" name="DeviceParameterValueText" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A parameter value where the value kind is string.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="DeviceParameterValue"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeviceParameterVendorName" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Device parameter's name used by a vendor of the device model, that is equivalent to the standardized parameter name.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeviceRelationshipKindCode" type="cbrncl:DeviceRelationshipKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Kind of relationship between devices. Note that device is used in a generic sense to encompass assembly levels from a system down to an atomic component.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeviceRelationshipStatusCode" type="cbrncl:DeviceRelationshipStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the status of a relationship between two devices.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DocumentPrivacyActIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if a document is categorized as containing personal information subject to protection by the Privacy Act; false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DwellDuration" type="niem-xsd:duration" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Dwell time per sweep (used only for an MCS spectrum), in ISO 8601 format.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ElapsedSweepsQuantity" type="niem-xsd:nonNegativeInteger" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Number of sweeps (used only if spectrumKindCode is MCS)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Elevation" type="nc:GeographicElevationMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A measure of the distance of a point on the Earth from sea level.  If not specified, units are in meters.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ElevationOffset" type="cbrn:DecimalListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The elevation offset angle from an expected bearing to the actual bearing . Angular measurement is in degrees minutes seconds. An angle above the expected bearing is positive and below is negative.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:UserName" name="EmployeeIdentifier" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier for a person in the form of an employee id.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="UserName"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:EncounterDeviceIdentification" name="EncounterDeviceAssetID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A device identifier that may be assigned by an organization for asset/property management and maintenance services.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="EncounterDeviceIdentification"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceCategoryCode" type="cbrncl:EncounterDeviceCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of a kind of encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceCategoryLevelCode" type="cbrncl:EncounterDeviceCategoryLevelCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the configuration level of a kind of encounter device; i.e., system, device, or component.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceConfiguration" type="cbrn:EncounterDeviceConfigurationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Encounter device configuration parameters.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceCoordinates" type="cbrn:ThreeDimensionalGeographicCoordinateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Geographical coordinates (latitude, longitude, optional elevation) of an encounter device location at the time a measurement was performed.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceHealthStatusCode" type="cbrncl:EncounterDeviceHealthStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of the health status of an encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:EncounterDeviceIdentification" name="EncounterDeviceID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A device identifier that is specific to its deployment site or operating organization. There is no required format for the ID value.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="EncounterDeviceIdentification"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceIdentification" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of the device that is the starting point/origin of the relationship.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceLocation" type="cbrn:EncounterDeviceLocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Geometric data regarding the location of an encounter device, and location of the measured item with respect to the location of the encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceLocationName" type="nc:ProperNameTextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Name of the physical location where a measurement was performed by an encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceMeasurementID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier generated by the encounter device for a block of its measurement data.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:EncounterDeviceIdentification" name="EncounterDeviceMfgID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A device identifier that is assigned by the manufacturer, typically a serial number.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="EncounterDeviceIdentification"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceOperatorComments" type="cbrn:EncounterDeviceOperatorCommentsType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides general comments by the operator of the encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceOperatorText" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information that identifies the encounter device's operator.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceProbe" type="cbrn:EncounterDeviceProbeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifies the kind of probe used by an encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceProbeID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A probe's serial number. There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceProbeKindCode" type="cbrncl:EncounterDeviceProbeKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifies a kind of probe, and the substance it detects.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceProbeModelName" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A probe model name.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceQualityControl" type="cbrn:DeviceQualityControlType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the quality control status of an encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceSpatialOrientation" type="cbrn:SpatialOrientationMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An encounter device's spatial orientation with respect to True North, the horizontal axis, and the angle of the device's vertical axis from the ellipsoid vertical axis; i.e., pitch, yaw, and roll.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceStatus" type="cbrn:EncounterDeviceStatusType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Status information for an encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceStatusDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date and time of the reported status state (ISO 8601).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceStatusParameters" type="cbrn:ParameterSetType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of status parameters and their values.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceToItemBearing" type="cbrn:BearingOffsetMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Azimuth and elevation from an encounter device center of mass to the estimated center of the measured item.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceToItemDistance" type="nc:LengthMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Distance from an encounter device to the item being measured. For a single-element detector, the EncounterDeviceToItemDistance is equal to the distance between the face of the detector and the estimated center of the source. If the source is inside a conveyance, the value of EncounterDeviceToItemDistance is equal to the distance from the face of the detector to the center of the conveyance, unless the source location can be determined more accurately. If the element EncounterDeviceToItemDistance is specified for a portal, the value equals the distance between the midpoint of the portal and the face of closest detector.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterDeviceVersion" type="cbrn:EncounterDeviceVersionType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Version information for an encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EncounterRelationship" type="cbrn:EncounterRelationshipType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of a relationship between two objects involved in an encounter.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EnergyCalibrationAdjustCode" type="cbrncl:EnergyCalibrationAdjustCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Calibration source to be used as a basis for the determination of energy calibration parameters.  If this element is missing, the spectrum is not to be used for automated adjustments to the energy calibration.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EnergyCalibrationAdjustIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the EnergyCalibrationAdjust method was applied to the processed data, otherwise false.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EnergyDeviationValues" type="cbrn:DoubleListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A white-space delimited list of double that provides the energy deviation values.  Deviation values are in pairs. The first number of each pair is actual energy (in keV); the second is the deviation (keV) from the actual energy of the adjusted nominal energy calibration. The deviation is positive if the actual energy exceeds the energy determined by the calibration parameters.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EntryPersonID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of the person who entered or confirmed a measured item identifier.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Equation" type="cbrn:EquationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The equation governing a calibration and its coefficients.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EquationCoefficients" type="cbrn:CoefficientsType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>List of values of the coefficients of an equation.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EquationCovarianceMatrix" type="cbrn:DoubleListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A white-space delimited list of values that provide the lower triangular half of an equation covariance matrix.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EquationDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A free-form description of the equation type; it is intended for documentation purposes only.  If the equation kind is Other, then this description shall be specified to explain the kind of equation.   Polynomial equations shall report the linear term first as Term0 followed by the other terms.  An example would be: Energy = Term0 + Term1*Ch + Term2*Ch**2</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EquationKindCode" type="cbrncl:EquationKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Kind of an equation.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ErrorCodeDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Text description of an error code.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ErrorCodeText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An error code.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ErrorDescription" type="cbrn:MessageErrorType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A text description of an error that occurred at a specific XML tag while processing an XML message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ErrorNodeName" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The XML tag at which an error occurred.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EventOnsetDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date and time of the start of an Event (ISO 8601 format).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FileCaptureDeviceCategoryCode" type="cbrncl:EncounterDeviceCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The kind of device that captured/recorded a data file.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FileCaptureDeviceID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of the device that captured/recorded a data file.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FileCaptureDuration" type="niem-xsd:duration" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Total duration of time covered by the data recorded in a digital data file (in ISO 8601 format).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FileCaptureStartTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date/time when recording of the data in a digital data file began (in ISO 8601 UTC format).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FileSet" type="cbrn:FileSetType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A logical collection of digital data files that are related in some manner.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FileSetDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of a file set.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FileSetMember" type="cbrn:FileSetMemberType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A digit file that is a member of a file set.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FileSetName" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Name of a file set.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FileSetQuantity" type="niem-xsd:nonNegativeInteger" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The quantity of files contained in a Case.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FileSetUUID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Unique identifier of a file set.  Also serves as the DataFileUUID of a file containing the identifiers of the members of the file set.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="GeoPosition" type="cbrn:ThreeDimensionalGeographicCoordinateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Geographic position information for a location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="GeographicCoordinates" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for geographic coordinates.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="GrossCountData" type="cbrn:GrossCountDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The measurement data from a gross count-capable encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="GrossCountQuantity" type="cbrn:NonNegativeIntegerListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>White space delimited list of gross count data, one value per sample.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="IdAcquisitionMethodCode" type="cbrncl:IdAcquisitionMethodCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The method by which a measured item identifier was determined.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="IdConfidenceCode" type="cbrncl:ConfidenceCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the confidence that the measured item identifier is correct.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ImagePerspectiveCode" type="cbrncl:ImagePerspectiveCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the viewing perspective of the subject of an image captured as a digital data file.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="InServiceIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if an encounter device is currently certified as being properly calibrated and considered in service; false otherwise. If this element is missing, the meaning is "unknown".</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="InspectionActivityCode" type="cbrncl:InspectionActivityCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description the inspection activity during which a radiation detection alarm may occur.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="InspectionData" type="cbrn:InspectionDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information regarding an inspection of an item of interest.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="InspectionDate" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date at which an encounter device was most recently inspected and verified as being available for use.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="InspectionEventDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>DateTime of an inspection event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="InspectionResolutionCode" type="cbrncl:InspectionResolutionCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the findings resulting from inspection of an item of interest.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="InspectionThreatFindingCode" type="cbrncl:InspectionThreatFindingCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Category of threat found by an inspection.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ItemDispositionStatusCode" type="cbrncl:ItemDispositionStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the current disposition of an item that is the subject of an inspection(s)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ItemPosition" type="nc:LengthMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The position is the distance between the front of the item being measured at the start of the measurement and the centerline of the encounter device doing the measurement. The item front is the side in the direction of the item's movement. If the item is not in motion, the front needs to be explained in the MeasuredItemGeometryDescriptionText.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:SpectrumID" name="ItemSpectrumID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier for a spectrum measurement, where RadSourceKindCode is Item or Other.  There is no required format for the ID value.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="SpectrumID"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ItemSpeed" type="cbrn:VelocityMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Speed of a measured item (vehicle, train, boat, etc.) at the time of a measurement.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Latitude" type="nc:LatitudeCoordinateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A measurement of the angular distance between a point on the Earth and the Equator. Value is in the form of +/-dd:mm:ss where dd is restricted to 00 (inclusive) to 90 (exclusive), and mm and ss are restricted to 00 (inclusive) to 60 (exclusive).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Layer" type="cbrn:LayerType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides data on a shielding layer.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="LayerDensityValue" type="cbrn:DoubleType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The density of the shielding layer expressed in unit of measure g/cm2.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="LayerMaterialDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Text description of a layer's material.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="LayerMaterialKindCode" type="cbrncl:LayerMaterialKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The kind of material of which a shielding layer is composed. If the kind is "other", then a text description should also be provided.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="LayerQuantity" type="cbrn:PositiveIntegerType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The number of shielding layers observed.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="LayerSequenceNumber" type="cbrn:PositiveIntegerType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The sequence of a layer, if there is more than one layer. The outermost layer is "1".</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="LiveTimeDuration" type="niem-xsd:duration" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Effective time duration of a measurement in ISO 8601 format.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="LocationDirectionsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>General directions to a location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Longitude" type="nc:LongitudeCoordinateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A measurement of the angular distance between a point on the Earth and the Prime Meridian. Value is in the form of +/-dd:mm:ss where dd is restricted to 00 (inclusive) to 90 (exclusive), and mm and ss are restricted to 00 (inclusive) to 60 (exclusive).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="LowerLimitValue" type="cbrn:DoubleType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The lowest value of X for which an equation is valid.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MIMEContentCode" type="cbrncl:MIMEContentCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>MIME content type of a digital data file.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MIMEEncodingCode" type="cbrncl:MIMEEncodingCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Encoding MIME type of a digital data file.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MapGuideBrandCode" type="cbrncl:MapGuideBrandCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Brand name of a Map Guide document that provides maps of a locale with a vendor-unique grid reference system.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MapGuideGridNumber" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A map grid reference in a Map Guide document.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MapGuideLocation" type="cbrn:MapGuideLocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides location information based on a map contained in a Map Guide document.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MapGuideName" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Name of a Map Guide document providing maps of a locale.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MapGuidePageNumber" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A page reference in a Map Guide document.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MaximumProcessedDataValue" type="cbrn:DoubleType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Maximum standard deviation of some processed data values compared to the background processed data values.  For example, energy-window values from some proprietary algorithm.  With the maximum value reported, re-scoring with another threshold value is facilitated and avoids the necessity of disclosing the algorithm details.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MaximumSigmaValue" type="cbrn:DoubleType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Maximum standard deviation of the gross count values compared to the background count values.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MeanPhotonValue" type="niem-xsd:decimal" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Mean of the maximum number of photons in open air per pixel. Can be per system or per detector.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MeasuredItem" type="cbrn:MeasuredItemType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides information on a measured item.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MeasuredItemDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the measured item.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MeasuredItemGeometryDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the measured item geometry that can aid in understanding of the location, shape and size of the measured item.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MeasuredItemID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of a measured item.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MeasuredItemIDKindCode" type="cbrncl:MeasuredItemIDKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A kind of identifier for a measured item.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MeasuredItemIdentification" type="cbrn:MeasuredItemIdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identification information for  the measured item, such as bar code label or container number.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MeasuredItemKindCode" type="cbrncl:MeasuredItemKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifies the kind of item being inspected for Radiation/Nuclear threats.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MeasuredItemQuantity" type="nc:QuantityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Quantity or size of an item and the uncertainty in that value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MeasuredItemReferenceDate" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Reference date for radionuclide activity calculations (i.e., activities decay-corrected to this date).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MeasurementStartDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date/time of the start of the measurement by the encounter device (ISO 8601 UTC).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MeasurementStopDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date/time of the end of the measurement by the encounter device (ISO 8601 UTC).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageContentError" type="cbrn:MessageContentErrorType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides information about the point in the xml payload content of a message where an error occurred in processing the message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageContentHeader" type="cbrn:ContentHeaderType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Common payload header information for a  N25 message. This is independent of a specific transmission protocol that may be used to wrap the payload.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageCreationDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The timestamp associated with the creation of a message content header.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageDestination" type="cbrn:MessageOriginOrDestinationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>the identifier and/or name of a facility, site, or organization(s) that is(are) the destination of a message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageDispatchDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The timestamp associated with the dispatch of a message content and its header to a messaging service.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageHandlingError" type="cbrn:MessageErrorType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes a message error encountered by an infrastructure component in the process of message handling and transmission.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The message identifier associated with a message content.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageKindCode" type="cbrncl:MessageKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The kind of message content associated with a content header.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageOrigin" type="cbrn:MessageOriginOrDestinationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Facility, site, or organization id and/or name from which a message content originated.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageOriginOrDestination" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept to identify the origin or destination of a message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessagePriorityCode" type="cbrncl:MessagePriorityCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Priority of the message content associated with a content header.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageStatus" type="cbrn:MessageStatusType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides success or error feedback on a message that has been received.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageStatusCode" type="cbrncl:MessageStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the receiving status of a message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageVersionText" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The version of the message content kind associated with a content header.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NRCLicenseID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of the NRC license, if any.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NetCountRateValues" type="cbrn:DoubleListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A white-space delimited list of values providing the net count rates during the duration of the measurement; i.e., (gross counts - background counts)/time (in seconds).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NonLinearityCorrection" type="cbrn:NonLinearityCorrectionType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Nonlinearities in the correlation between energy and channel number.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NonLinearityCorrectionIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the NonLinearityCorrection method was applied to the processed data; false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Nuclide" type="cbrn:NuclideType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identification of a nuclide.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NuclideActivityValue" type="cbrn:NuclideActivityMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A nuclide's calculated activity value and its uncertainty.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NuclideCategoryCode" type="cbrncl:NuclideCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A category of nuclide.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NuclideCode" type="cbrncl:NuclideCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A standard code for a nuclide.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NuclideEstimatedPosition" type="cbrn:RelativePositionType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The estimated position of a nuclide in a measured item.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NuclideEstimatedShielding" type="cbrn:NuclideEstimatedShieldingType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides information on shielding.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NuclideIdConfidence" type="cbrn:IdentificationConfidenceType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A characterization of the confidence that a nuclide identification is correct.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NuclideIdentification" type="cbrn:NuclideIdentificationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Nuclide identified by the analysis of encounter device data.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NuclideSourceID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of the Nuclide source item.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OffsetXValue" type="niem-xsd:decimal" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Value of the X dimension offset from the origin.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OffsetYValue" type="niem-xsd:decimal" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Value of the Y dimension offset from the origin.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OffsetZValue" type="niem-xsd:decimal" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Value of the Z dimension offset from the origin.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OperationalIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the encounter device is operational; false otherwise. Indicates the operational status of an encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ParameterName" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The name of a parameter.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ParameterNames" type="cbrn:StringListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of parameter names in a parameter set.  The order of the values and names must be the same.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ParameterSet" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a set of parameters.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:ParameterSet" name="ParameterSetList" type="cbrn:ParameterSetListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A set of parameters and their values.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="ParameterSet"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:ParameterSet" name="ParameterSetPair" type="cbrn:ParameterSetPairType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A set of parameters and their values.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="ParameterSet"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ParameterValueText" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The value of a parameter, expressed as a string.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ParameterValueUnitsText" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The unit of measure in which a parameter value is expressed.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ParameterValues" type="cbrn:DecimalListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of  parameter values in a parameter set.  The order of the values and names must be the same.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ParentDeviceCategoryCode" type="cbrncl:EncounterDeviceCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The category of the device that is the parent of the device identified by rn:RadEncounterDeviceID.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ParentDeviceID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The identifier of the device that is the parent of the device identified by rn:RadEncounterDeviceID.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element substitutionGroup="cbrn:UserName" name="PersonName" type="nc:PersonNameType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier for a person in the form of a person's actual name.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="UserName"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PhotonData" type="cbrn:PhotonDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Data providing the photon properties of a radiographic device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PhotonEnergyValue" type="niem-xsd:decimal" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Mean energy of photons in MeV.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PhotonSourceCode" type="cbrncl:PhotonSourceCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the photon source for a radiographic device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PhotopeakContinuumValue" type="niem-xsd:nonNegativeInteger" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The sum total of background counts in the continuum beneath the photopeak. The PhotopeakContinuumValue should reflect the sum total counts beneath the photopeak that span an area equal to three FWHM units centered about the photopeak centroid.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PhotopeakData" type="cbrn:PhotopeakDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Photopeak data resulting from analysis of a gamma ray spectrum.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PhotopeakEnergyValue" type="niem-xsd:decimal" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Energy value of the photopeak.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PhotopeakIntensityValue" type="niem-xsd:decimal" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The gamma ray intensity of the nuclide-specific photopeak (a.k.a. the branching ratio or emission probability) as a fractional value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PhotopeakLibraryName" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Name of a Photopeak library.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PhotopeakNetAreaValue" type="cbrn:DoubleUncType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Net photopeak counts with the uncertainty (i.e.,  one sigma net error associated with the PhotopeakNetAreaValue).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PhotopeakNuclide" type="cbrn:PhotopeakNuclideType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifies the nuclide associated with the photopeak energy.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PhotopeakResolutionValue" type="niem-xsd:decimal" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A photopeak FWHM resolution value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PhotopeakSigmaValue" type="niem-xsd:decimal" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A photopeak significance value measured in sigma units.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PointXY" type="cbrn:PointXYType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A single two dimensional - i.e., (X,Y) - data point.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PositionID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Position identifier of an encounter device that is deployed in a designed position, such as an inspection lane.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PositiveIntegerList" type="cbrn:PositiveIntegerListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A white space-delimited list of positive integers.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PriorVersionID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Version Identifier of the version immediately prior to the current version. There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ProcessedChannelData" type="cbrn:SpectrumChannelDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Spectrum channel count data aggregated from raw spectrum using a processing method described in ProcessedSpectrumDescriptionText.  The first channel number shall always be 0.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ProcessedGrossCountData" type="cbrn:ProcessedGrossCountDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides net count rate data resulting from processing a set of raw gross counts to prepare it for analysis and assessment.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ProcessedGrossCountDataDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Descriptive text about the methods employed to produce the processed gross count data.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ProcessedSpectrumDataReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A reference to a processed spectrum and gross count data used in an analysis.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="ProcessedSpectrumDataType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ProcessedSpectrumData" type="cbrn:ProcessedSpectrumDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A reference to a processed spectrum and gross count data used in an analysis.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ProcessedSpectrumDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the processed spectrum.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ProcessedSpectrumID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier for a processed spectrum measurement.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ProcessedSpectrumImageReference" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A vector-graphics image of the spectral data after it has been processed.  Spectral images shall not include assessment or analysis results in the image. The svg file should be compressed using gzip or equivalent.</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="ProcessedSpectrumImageType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ProcessedSpectrumImage" type="cbrn:ProcessedSpectrumImageType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A vector-graphics image of the spectral data after it has been processed.  Spectral images shall not include assessment or analysis results in the image. The svg file should be compressed using gzip or equivalent.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadAbsorbedDoseRateValues" type="cbrn:RadAbsorbedDoseRateMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of radiation absorbed dose rate values presented in absorbed dose units per hour (e.g. rad/hr, mGy/hr).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadAlarmKindCode" type="cbrncl:RadAlarmKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of a kind of radiation alarm.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadAlarmLightCode" type="cbrncl:RadAlarmLightCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The alarm light color, if the alarm is annunciated by lights.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadAlarmSignalStrengthValue" type="niem-xsd:decimal" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The highest signal index value produced by the device during an alarm state.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadCountRateValues" type="cbrn:RadCountRateMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of count rate values from a radiation dose rate detector.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadCountValues" type="cbrn:NonNegativeIntegerListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of raw counts from a radiation dose rate detector.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadDoseRateValues" type="cbrn:RadDoseRateMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of radiation dose rate values presented in dose units per hour (e.g. rem/hr, Sv/hr).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadEncounterDeviceProcessedData" type="cbrn:RadEncounterDeviceProcessedDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Data that results from processing raw measurement data by an encounter device in some manner as a basis for performing automated analysis of the data.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadEncounterDeviceRawData" type="cbrn:RadEncounterDeviceRawDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The raw (unprocessed) data measured by gross count or spectroscopic measuring encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadEncounterDeviceToShieldingDistance" type="nc:LengthMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Distance from the center of mass of a encounter device to the closest outside portion of the outermost layer of shielding that is found between the encounter device and the radiation source.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadExposureRateValues" type="cbrn:RadExposureRateMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of radiation exposure rate values presented in exposure units per hour (e.g. R/hr, uR/hr).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadSourceActivity" type="cbrn:NuclideActivityMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information on encounter device nuclide activity readings that are manually entered by an operator.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadSourceKindCode" type="cbrncl:RadSourceKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Kind of radiation source for a device measurement.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadTotalAbsorbedDoseValues" type="cbrn:RadAbsorbedDoseMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of total (accumulated) radiation absorbed dose values.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadTotalDoseValues" type="cbrn:RadlDoseMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of total (accumulated) radiation dose values.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RadTotalExposureValues" type="cbrn:RadExposureMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of total (accumulated) radiation exposure values.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RawDataProcessingMethodDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A free-form description of the method used to produce processed data from the available raw data.  Used primarily for documentation purposes.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RawDataStartStopIDList" type="cbrn:StringListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A list of the identifiers of raw spectrum measurements that have been included in a processed spectrum.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ReachbackData" type="cbrn:ReachbackDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information regarding Reachback.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ReachbackFindingsText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of reachback findings.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ReachbackReasonText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A text description of the reason for a reachback request.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ReachbackRecommendationCode" type="cbrncl:ReachbackRecommendationCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of a reachback recommendation.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ReachbackRecommendationDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>DateTime that a reachback recommendation was made</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ReachbackRequestCode" type="cbrncl:ReachbackRequestCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of a reachback request.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RealTimeDuration" type="niem-xsd:duration" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Effective real time duration of the measurement in ISO 8601 format.  For processed data this can represent the real times summed over multiple counts and/or multiple detectors.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RefAlarmID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of a set of alarm data. There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RefBackgroundSpectrumID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Reference to a background spectrum.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RefCalibrationSpectrumID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Reference to a calibration spectrum.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RefItemSpectrumID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier for the item spectrum measurement data associated with a gross count measurement.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RelatedCase" type="cbrn:CaseRelationshipType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A case that is related to a case of interest.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RelatedCaseUUID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The identifier of a case that is related in some manner to a case of interest.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RelationshipBeginDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>DateTime a relationship began.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RelationshipEndDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>DateTime a relationship ended.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RelativePositionOriginText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>User/operator entered description of the origin point from which the relative position offset values are measured.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RemarkText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>General comments or remark.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Report" type="cbrn:ReportType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A report provided on an unsolicited basis; i.e., not in response to a request message (Pull), but by Push from the entity providing the report.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ReportDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>DateTime when a report was created.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RequestAgency" type="cbrn:RequestAgencyType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The organization that initiated a request.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RequestAgencyCode" type="cbrncl:GNDAOrgCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The organization from which a request was initiated.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RequestData" type="cbrn:RequestDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information identifying the request for which a message provides a response.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RequestDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>DateTime of a request.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RequestID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of a request.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RequestScopeCode" type="cbrncl:RequestScopeCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Describes the scope of a request.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResendRequestIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if the message should be resent; false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResponseReport" type="cbrn:ResponseReportType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A report provided in response to a request.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RotationalOffset" type="cbrn:DecimalListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An item's spatial orientation as the rotational angle around its centerline from rear to front.  Angular measurement is in degrees minutes seconds, measured looking in the direction of rear to front. The maximum angle is 180 degrees; clockwise is positive and counterclockwise is negative.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ScanIdentificationData" type="cbrn:ScanIdentifierDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Scan information regarding an item that is inspected.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ScanIdentifierData" type="cbrn:ScanIdentifierDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identification data about a scan of an item of interest using a detection device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ScanQualityCode" type="cbrncl:ScanQualityCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the quality of a scan.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SecondaryInspectionKindCode" type="cbrncl:SecondaryInspectionKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Kind of secondary Inspection.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SecondaryInspectionReferral" type="cbrn:SecondaryInspectionReferralType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information regarding a secondary inspection referral for an item of interest.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SecondaryInspectionReferralID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of a Secondary Inspection referral.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SecondaryInspectionReferralReasonCode" type="cbrncl:SecondaryInspectionReferralReasonCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Reason why Secondary Inspection was recommended.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SecondaryInspectionResolution" type="cbrn:SecondaryInspectionResolutionType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information regarding the resolution of Secondary Inspection of an item of interest.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SecurityClassificationCode" type="cbrncl:SecurityCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of a security classification.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Shielding" type="cbrn:ShieldingType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides user data entries that describe the shielding observed by the inspector(s) as relevant to the measured item.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SimplexIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>If the association is describing a communications connection/link, then the link is simplex if True; else False if duplex.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SiteLocation" type="cbrn:SiteLocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information about the location of a site.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SiteMobileDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A text description of a site that is mobile.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SiteMobileIndicator" type="niem-xsd:boolean" nillable="true">
    <xsd:annotation>
      <xsd:documentation>True if a site is mobile; false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SiteSpecialInfoText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Information about a site that may be of importance to a responder; e.g.,  electrified fence, dogs on property, loft apartment, multi-storied building, multiple warehouses on site, hazardous material, etc.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SourceSystemFindingCategoryText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An InspectionThreatFindingCode mapped to the finding category in the source system.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SpectrumChannelData" type="cbrn:SpectrumChannelDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Spectrum measurement values expressed as counts by channel for the entire spectrum or a region of interest.  The first channel number shall always be 0.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SpectrumData" type="cbrn:SpectrumDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The measurement data from a spectagraphic-capable encounter device.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SpectrumID" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for an identifier of a spectrum.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SpectrumImageDataFile" type="cbrn:DataFileType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A processed spectrum image in scalar vector graphics (svg) data format.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SpectrumOverflowValue" type="niem-xsd:nonNegativeInteger" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A value that presents the sum total counts of all gamma ray interactions in the spectrometer whose energy exceeds the energy of the last channel in the spectrum. For example, if the calibrated energy of the 1024th channel in a 1024 channel spectrum is equal to 3000 keV, this element will represent all pulses that were measured whose energy is greater than 3000 keV.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SystemEvent" type="cbrn:SystemEventType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Basic data elements of a system event for audit reporting purposes.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SystemEventDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date/Time of a system event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TemplateLibraryName" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Name of a template library.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TemplateRankedNuclide" type="cbrn:TemplateRankedNuclideType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A nuclide deemed by the algorithm applied in an analysis to be possibilities for identifying the correct nuclide(s) in the spectrum, but was not selected for reporting as an identified nuclide.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TemplateRankingData" type="cbrn:TemplateRankingDataType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Ranking of candidate nuclides as the nuclides in a spectrum, but not included in the list of identified nuclides.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ThreatDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of the threat determined by the analysis of the data collected in a detection event.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ThreatLevelDeterminationCode" type="cbrncl:ThreatLevelDeterminationCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Threat level based on findings during an inspection.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ToDeviceID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of the device that is the ending point/target of the relationship.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Traversal" type="cbrn:TraversalType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The end-to-end workflow of a conveyance through an encounter process. One or more detection events may occur during a traversal.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TraversalBeginDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>DateTime of the start of a traversal.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TraversalEndDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>DateTime of the end of a traversal.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TraversalID" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of a traversal.  There is no required format for the ID value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TraversalOperatingModeCode" type="cbrncl:TraversalOperatingModeCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Traversal operating mode associated with an acknowledgement.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TrueNorthOffset" type="cbrn:DecimalListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An item's spatial orientation with respect to the angular offset from true North, i.e., compass heading from true North. Angular measurement is in degrees minutes seconds.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="UpperLimitValue" type="cbrn:DoubleType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The highest value of X for which an equation is valid.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="UserName" abstract="true">
    <xsd:annotation>
      <xsd:documentation>A data concept for a name used to identify a user who enters data or performs a function on an information system.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VelocityUnitCode" type="unece:VelocityCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Units of measure for velocity (speed).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VersionEffectiveDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Version effectivity dateTime.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VersionIdentifier" type="niem-xsd:string" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifier of a version.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VersionKindCode" type="cbrncl:VersionKindCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A kind of physical implementation of a version, e.g., software, firmware, hardware, other.  If "other", this should be explained in the algorithm description.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VerticalOffset" type="cbrn:DecimalListType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An item's spatial orientation with respect to the angular offset from the vertical axis. Angular measurement is in degrees minutes seconds. An angle above the horizontal is positive and and below the horizontal is negative.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VoltageAppliedValue" type="niem-xsd:integer" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The value of the voltage applied to the detector during a calibration measurement. Units are volts.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="XDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Text descripion of the first dimension's data.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="XValue" type="cbrn:DoubleUncType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Value (and optionally, the 1-sigma absolute uncertainty of this value), of the first dimension.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="YDescriptionText" type="nc:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Text descripion of the second dimension's data.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="YValue" type="cbrn:DoubleUncType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Value (and optionally, the 1-sigma absolute uncertainty of this value), of the second dimension.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
</xsd:schema>

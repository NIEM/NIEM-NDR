      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:MetadataType">
        <xsd:sequence>
          <xsd:element ref="cbrn:VersionIdentifier" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:VersionEffectiveDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:SecurityClassificationCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DocumentPrivacyActIndicator" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ClassificationReasonText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ClassifiedByText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DeclassDate" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DeclassEventText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DeclassExceptionText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DeclassManualReviewIndicator" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="DataFileSequenceType">
    <xsd:annotation>
      <xsd:documentation>A data type providing information on a subset of data within a digital data file.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:DataFileSequenceIdentifier" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DataFileSequenceDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ImagePerspectiveCode" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="DataFileType">
    <xsd:annotation>
      <xsd:documentation>A data type that provides information for a digital data file and optionally the data file itself. This type applies to digital data files of any content and encoding method.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="BinaryType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:BinaryType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DetectionEventUUID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DataFileUUID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DataFileName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DataFileCaptureData" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DataFileCompressionDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DataFileSequence" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DataFileSubjectCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DataFileDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DataFileMetadata" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="DecimalListSimpleType">

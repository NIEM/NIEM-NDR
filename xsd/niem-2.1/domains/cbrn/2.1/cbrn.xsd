          <xsd:element ref="cbrn:EncounterDeviceID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="nc:ItemMakeName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="nc:ItemModelName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EncounterDeviceVersion" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DeviceInfoDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ParentDeviceCategoryCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ParentDeviceID" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="DeviceParameterType">
    <xsd:annotation>
      <xsd:documentation>A data type for a readable and-or settable parameter of a device.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:DeviceParameterName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DeviceParameterVendorName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DeviceParameterSettableIndicator" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DeviceParameterStatusIndicator" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DeviceParameterValue" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="cbrn:parameterUnitsText" use="optional"/>
      </xsd:extension>
    </xsd:complexContent>

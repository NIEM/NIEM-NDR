      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EncounterDeviceLocationName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EncounterDeviceCoordinates" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EncounterDeviceToItemDistance" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EncounterDeviceToItemBearing" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EncounterDeviceSpatialOrientation" minOccurs="0" maxOccurs="unbounded"/>

  <xsd:complexType name="DoubleListType">
    <xsd:annotation>
      <xsd:documentation>A data type for a white space-delimited list of double.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="cbrn:DoubleListSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="DoubleType">
    <xsd:annotation>
      <xsd:documentation>Proxy of xsd:double</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:double">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="DoubleUncType">
    <xsd:annotation>
      <xsd:documentation>A data type for a list of double with length of 2, where the first item in the list is the value and the second is the 1-sigma absolute total uncertainty (i.e., including all sources of uncertainty) in the value. An uncertainty value of 0 means unknown.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="DoubleListType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="cbrn:DoubleListType"/>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="EncounterDeviceConfigurationType">

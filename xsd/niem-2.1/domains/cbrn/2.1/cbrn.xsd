  <xsd:complexType name="DeviceQualityControlType">
    <xsd:annotation>
      <xsd:documentation>A data type for an encounter device's quality control information.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:InspectionDate" minOccurs="0" maxOccurs="unbounded"/>

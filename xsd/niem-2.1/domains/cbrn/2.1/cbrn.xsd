          <xsd:element ref="cbrn:AlarmDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RadAlarmLightCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:AlarmAudibleIndicator" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:AlarmDescriptionCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RadAlarmKindCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:AlarmDuration" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RadAlarmSignalStrengthValue" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DetectorDesignatorText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:InspectionActivityCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:AlarmAcknowledgementCode" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="cbrn:convergenceDuration" use="optional"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="DetectionEventAnalysisResultsType">
    <xsd:annotation>
      <xsd:documentation>A data type for detection event analysis results.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>

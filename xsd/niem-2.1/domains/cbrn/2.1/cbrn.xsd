<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://niem.gov/niem/domains/cbrn/2.1" version="1" xmlns:i2="http://niem.gov/niem/appinfo/2.1" xmlns:niem-xsd="http://niem.gov/niem/proxy/xsd/2.0" xmlns:i="http://niem.gov/niem/appinfo/2.0" xmlns:unece="http://niem.gov/niem/unece_rec20-misc/2.0" xmlns:cbrn="http://niem.gov/niem/domains/cbrn/2.1" xmlns:s="http://niem.gov/niem/structures/2.0" xmlns:cbrncl="http://niem.gov/niem/cbrncl/2.1" xmlns:scr="http://niem.gov/niem/domains/screening/2.1" xmlns:it="http://niem.gov/niem/domains/internationalTrade/2.1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:nc="http://niem.gov/niem/niem-core/2.0">

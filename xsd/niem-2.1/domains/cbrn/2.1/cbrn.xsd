          <xsd:element ref="cbrn:FileCaptureStartTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:FileCaptureDuration" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:FileCaptureDeviceID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:FileCaptureDeviceCategoryCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MIMEEncodingCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MIMEContentCode" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="DataFileMetadataType">

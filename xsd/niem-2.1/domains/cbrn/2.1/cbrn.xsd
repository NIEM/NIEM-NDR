  <xsd:complexType name="EncounterDeviceOperatorCommentsType">
    <xsd:annotation>
      <xsd:documentation>A data type for encounter device operator comments for general purpose devices employed in an encounter.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:DeviceOperatorID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DeviceOperatorComments" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="EncounterDeviceProbeType">
    <xsd:annotation>
      <xsd:documentation>A data type for a encounter device's probe.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:EncounterDeviceProbeKindCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EncounterDeviceProbeModelName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EncounterDeviceProbeID" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="EncounterDeviceStatusType">
    <xsd:annotation>
      <xsd:documentation>A data type for the status of an encounter device.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="StatusType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="nc:StatusType">
        <xsd:sequence>
          <xsd:element ref="cbrn:EncounterDeviceStatusDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:OperationalIndicator" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EncounterDeviceHealthStatusCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EncounterDeviceStatusParameters" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="EncounterDeviceVersionType">
    <xsd:annotation>
      <xsd:documentation>A data type that provides version identification information on an encounter device.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:VersionKindCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:CurrentVersionID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:PriorVersionID" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="EncounterRelationshipType">
    <xsd:annotation>
      <xsd:documentation>A data type that provides a means to define a relationship between two objects involved in an encounter.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RelationshipBeginDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RelationshipEndDateTime" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="EquationType">
    <xsd:annotation>
      <xsd:documentation>A data type that describes a mathematical equation and its coefficients.  The type of the equation is given by the equationKind attribute.  The coefficients of the equation are supplied by the EquationCoefficients element; the values of the covariance matrix may be supplied by the EquationCovarianceMatrix.  It is recommended that the data from which the equation coefficients were derived be made available as an ArrayXY type so that the coefficients for a different model could be derived.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EquationCoefficients" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EquationCovarianceMatrix" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EquationKindCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EquationDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:UpperLimitValue" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:LowerLimitValue" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="FileSetMemberType">
    <xsd:annotation>
      <xsd:documentation>A data type identifying a member of a file set.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:DataFileUUID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DataFileName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MIMEContentCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DataFileSubjectCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:SecurityClassificationCode" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="cbrn:sequenceNumber" use="optional"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="FileSetType">
    <xsd:annotation>
      <xsd:documentation>A data type for a logical collection of digital data files that are related in some manner.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:FileSetName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:FileSetDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:FileSetUUID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:FileSetQuantity" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:FileSetMember" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="GeographicCoordinatesType">
    <xsd:annotation>
      <xsd:documentation>A data type for geographic coordinates: first two entries are latitude and longitude; third entry (optional) is elevation relative to ellipsoid (in meters if units are not specified).</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:CoordinatesDateTime" minOccurs="0" maxOccurs="1"/>
          <xsd:element ref="cbrn:Latitude" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:Longitude" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:Elevation" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="nc:LengthUnitCode" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="cbrn:cepValue" use="optional"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="GrossCountDataType">
    <xsd:annotation>
      <xsd:documentation>A data type that provides the measurement data from a gross count-capable encounter device (e.g., gamma or neutron).</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DetectorDesignatorText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:DetectorKindText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:LiveTimeDuration" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:GrossCountQuantity" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:BackgroundCountValues" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RefItemSpectrumID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MaximumSigmaValue" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="cbrn:dataQualityCode" use="optional"/>
        <xsd:attribute ref="cbrn:endEnergyValue" use="optional"/>
        <xsd:attribute ref="cbrn:radEnergyUnitsCode" use="optional"/>
        <xsd:attribute ref="cbrn:startEnergyValue" use="optional"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="IdentificationConfidenceType">
    <xsd:annotation>
      <xsd:documentation>A data type for characterizing the confidence that an identification is correct.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:ConfidenceAssessment" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="InspectionDataType">
    <xsd:annotation>
      <xsd:documentation>A data type for information regarding an inspection of an item of interest.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:InspectionEventDateTime" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ScanIdentificationData" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ThreatLevelDeterminationCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:InspectionResolutionCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:UserName" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="LayerType">
    <xsd:annotation>
      <xsd:documentation>A data type for a shielding layer.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:LayerSequenceNumber" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="nc:ItemWidthMeasure" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:LayerDensityValue" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:LayerMaterialDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:LayerMaterialKindCode" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MapGuideLocationType">
    <xsd:annotation>
      <xsd:documentation>A data type for location information based on a Guide map published by a vendor.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:MapGuideBrandCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MapGuideName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MapGuidePageNumber" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MapGuideGridNumber" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MeasuredItemIdentificationType">
    <xsd:annotation>
      <xsd:documentation>A data type for an identifier of a measured item.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MeasuredItemID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MeasuredItemIDKindCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:IdAcquisitionMethodCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:IdConfidenceCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:EntryPersonID" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MeasuredItemType">
    <xsd:annotation>
      <xsd:documentation>A data type for a measured item that is the subject of a detection event.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:CargoDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MeasuredItemIdentification" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MeasuredItemKindCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MeasuredItemDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:NRCLicenseID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MeasuredItemQuantity" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MeasuredItemGeometryDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:RadSourceActivity" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MeasuredItemReferenceDate" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ItemDispositionStatusCode" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MessageContentErrorType">
    <xsd:annotation>
      <xsd:documentation>A data type that provides information about the point in the xml payload content of a message where an error occurred in processing the message.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:ErrorNodeName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ErrorDescription" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MessageErrorType">
    <xsd:annotation>
      <xsd:documentation>A data type that describes a message error.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:ErrorCodeText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ErrorCodeDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MessageOriginOrDestinationType">
    <xsd:annotation>
      <xsd:documentation>A data type for identifying a message origin or destination.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="it:FacilityID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="nc:FacilityName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="nc:OrganizationName" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="nc:OrganizationIdentification" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MessageStatusType">
    <xsd:annotation>
      <xsd:documentation>A data type for a message to provide success or error feedback on a message that has been received.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="SystemEventType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="cbrn:SystemEventType">
        <xsd:sequence>
          <xsd:element ref="cbrn:RemarkText" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MessageID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MessageKindCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MessageStatusCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MessageHandlingError" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ResendRequestIndicator" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MessageContentError" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MeasuredItemID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:MeasuredItemKindCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:TraversalID" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:CredentialsAuthenticatedCode" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="NonLinearityCorrectionType">
    <xsd:annotation>
      <xsd:documentation>A data type that identifies the nonlinearities in the correlation between the energy and channel number for the type of encounter device named.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:EnergyDeviationValues" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="NonNegativeIntegerListSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for a white space-delimited list of nonnegative integers.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:list itemType="xsd:nonNegativeInteger"/>
  </xsd:simpleType>
  <xsd:complexType name="NonNegativeIntegerListType">
    <xsd:annotation>
      <xsd:documentation>A data type for a white space-delimited list of nonnegative integers.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="cbrn:NonNegativeIntegerListSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="NuclideActivityMeasureType">
    <xsd:annotation>
      <xsd:documentation>A data type for a measurement of nuclide activity.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="DoubleUncType"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="cbrn:DoubleUncType">
        <xsd:attribute ref="cbrn:nuclideActivityUnitsCode" use="optional"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="NuclideEstimatedShieldingType">
    <xsd:annotation>
      <xsd:documentation>A data type to provide information on shielding based on analysis of the spectral data.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:AtomicNumberValue" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:ArealDensityValue" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="NuclideIdentificationType">
    <xsd:annotation>
      <xsd:documentation>A data type for information about a nuclide.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:Nuclide" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:NuclideCategoryCode" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:NuclideActivityValue" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:NuclideIdConfidence" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:NuclideEstimatedPosition" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="cbrn:NuclideEstimatedShielding" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="NuclideType">
    <xsd:annotation>
      <xsd:documentation>A data type for a nuclide.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element ref="cbrn:NuclideCode" minOccurs="0" maxOccurs="unbounded"/>

          <xsd:element ref="im:DependentJ2CompletionIndicator" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="im:DependentJ2TerminationIndicator" minOccurs="0" maxOccurs="unbounded"/>

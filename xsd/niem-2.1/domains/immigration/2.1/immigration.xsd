          <xsd:element ref="im:TotalInternationalOrganizationFundingSupportAmount" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="im:TotalOtherOrganizationFundingSupportAmount" minOccurs="0" maxOccurs="unbounded"/>

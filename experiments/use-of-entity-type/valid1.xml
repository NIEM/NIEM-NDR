<?xml version="1.0" encoding="us-ascii"?>
<!DOCTYPE instance:instance [
   <!NOTATION jpeg SYSTEM "JPG">
   <!ENTITY ALPHA SYSTEM "alpha" NDATA jpeg>
   <!ENTITY BETA SYSTEM "beta" NDATA jpeg>
   <!ELEMENT instance:instance ANY>
  <!ATTLIST instance:instance 
            xmlns:instance CDATA #IMPLIED
            xmlns:ns1 CDATA #IMPLIED
            xmlns:xsi CDATA #IMPLIED
            xsi:schemaLocation CDATA #IMPLIED
            ns1:test CDATA #IMPLIED
            ns1:test-list CDATA #IMPLIED
            >

]>
<instance:instance
   xmlns:instance="http://niem.gtri.gatech.edu/instance/1.0"
   xmlns:ns1="http://example.org/ns1"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="
      http://example.org/ns1 ns1.xsd
      http://niem.gtri.gatech.edu/instance/1.0 ../../xsd/instance-1.0.xsd"
   ns1:test="ALPHA"
   ns1:test-list="ALPHA BETA"
   ></instance:instance>
